<?xml version="1.0"?>
<PCF
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="../../../../../../pcf.xsd">
  <ListViewPanel
    border="true"
    editable="true"
    id="DBStatisticsRequestLV">
    <Require
      name="TablePickers"
      type="gw.api.tools.PickerRow[]"/>
    <RowIterator
      editable="true"
      elementName="Row"
      numDataEntriesPerRow="4"
      pageSize="60"
      type="gw.api.tools.PickerRow"
      value="TablePickers">
      <Row>
        <CheckBoxCell
          editable="true"
          id="selector"
          validationLabel="displaykey.Web.InternalTools.InfoPages.DataDistribution.TableChosen"
          value="Row.Chosen"/>
        <Cell
          id="tname"
          value="Row.DisplayName"/>
      </Row>
    </RowIterator>
  </ListViewPanel>
</PCF>
