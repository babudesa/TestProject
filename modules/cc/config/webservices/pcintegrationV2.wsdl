        <xsd:annotation>
          <xsd:documentation>
SOAP Exception specific to batch process operations     </xsd:documentation>
        </xsd:annotation>
        <complexContent>
          <extension base="tns2:SOAPServerException">
            <sequence/>
          </extension>
        </complexContent>
      </complexType>
