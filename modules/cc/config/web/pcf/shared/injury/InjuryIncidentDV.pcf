<?xml version="1.0"?>
<PCF
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="../../../../../../pcf.xsd">
  <DetailViewPanel
    id="InjuryIncidentDV">
    <Require
      name="InjuryIncident"
      type="InjuryIncident"/>
    <Variable
      initialValue="InjuryIncident.Claim"
      name="Claim"
      type="Claim"/>
    <InputColumn>
      <Label
        label="displaykey.Web.InjuryIncidentDV.Injured"/>
      <ClaimContactInput
        claim="Claim"
        editable="true"
        id="Injured_Picker"
        label="displaykey.Web.InjuryIncidentDV.Injured.Picker"
        newContactMenu="ClaimNewPersonOnlyPickerMenuItemSet"
        required="true"
        value="InjuryIncident.Injured"
        valueRange="Claim.RelatedPersonArray"/>
      <InputSetRef
        def="InjuryIncidentInputSet(InjuryIncident)"
        visible="true"/>
    </InputColumn>
  </DetailViewPanel>
</PCF>
