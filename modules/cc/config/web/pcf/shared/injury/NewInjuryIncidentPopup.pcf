<?xml version="1.0"?>
<PCF
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="../../../../../../pcf.xsd">
  <Popup
    afterEnter="if ((Exposure != null) &amp;&amp; (Exposure.claimant typeis Person)) { InjuryIncident.injured = Exposure.claimant as Person }"
    canEdit="true"
    canVisit="perm.Claim.createincident(Claim)"
    id="NewInjuryIncidentPopup"
    returnType="InjuryIncident"
    startInEditMode="true"
    title="displaykey.Web.NewInjuryIncident.Title">
    <LocationEntryPoint
      signature="NewInjuryIncidentPopup(Claim : Claim)"/>
    <LocationEntryPoint
      signature="NewInjuryIncidentPopup(Claim : Claim, Exposure : Exposure)"/>
    <Variable
      name="Claim"
      type="Claim"/>
    <Variable
      name="Exposure"
      type="Exposure"/>
    <Variable
      initialValue="Claim.newIncident(entity.InjuryIncident) as InjuryIncident"
      name="InjuryIncident"/>
    <Screen
      id="NewInjuryIncidentScreen">
      <TitleBar
        icon="InjuryIncident.HeaderIcon"/>
      <Toolbar
        reflectOnBottom="true">
        <EditButtons
          pickValue="InjuryIncident"/>
      </Toolbar>
      <PanelRef
        def="InjuryIncidentDV(InjuryIncident)"/>
    </Screen>
  </Popup>
</PCF>
