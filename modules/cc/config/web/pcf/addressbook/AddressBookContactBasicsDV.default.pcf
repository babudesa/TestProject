<?xml version="1.0"?>
<PCF
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="../../../../../pcf.xsd">
  <DetailViewPanel
    id="AddressBookContactBasicsDV"
    mode="default">
    <Require
      name="contact"
      type="Contact"/>
    <Require
      name="externalContactSource"
      type="contact.ExternalContactSource"/>
    <InputColumn>
      <Label
        label="displaykey.Web.ContactDetail.UndefinedType(contact.subType)"
        visible="contact != null"/>
      <Label
        label="displaykey.Web.ContactDetail.NullType"
        visible="contact == null"/>
    </InputColumn>
  </DetailViewPanel>
</PCF>
