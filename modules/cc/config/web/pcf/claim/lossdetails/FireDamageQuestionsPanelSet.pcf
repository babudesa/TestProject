<?xml version="1.0"?>
<PCF
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="../../../../../../pcf.xsd">
  <PanelSet
    id="FireDamageQuestionsPanelSet">
    <Require
      name="aClaim"
      type="Claim"/>
    <DetailViewPanel>
      <InputColumn>
        <BooleanRadioInput
          editable="true"
          id="ArsonInvolved"
          label="displaykey.NVV.Claim.SubView.LossDetailsProperty.Claim.FireDamage.ArsonInvolved"
          value="aClaim.PropertyFireDamage.Arson"/>
        <TextAreaInput
          editable="true"
          id="FireSource"
          label="displaykey.NVV.Claim.SubView.LossDetailsProperty.Claim.FireDamage.FireSource"
          numRows="2"
          value="aClaim.PropertyFireDamage.FireSource"/>
        <TextAreaInput
          editable="true"
          id="HowWasFireDiscovered"
          label="displaykey.NVV.Claim.SubView.LossDetailsProperty.Claim.FireDamage.HowWasFireDiscovered"
          numRows="2"
          value="aClaim.PropertyFireDamage.HowWasFireDiscovered"/>
        <BooleanRadioInput
          editable="true"
          id="FireDeptResponded"
          label="displaykey.NVV.Claim.SubView.LossDetailsProperty.Claim.FireDamage.FireDeptResponded"
          value="aClaim.PropertyFireDamage.FireDeptResponded"/>
      </InputColumn>
      <InputColumn>
        <BooleanRadioInput
          editable="true"
          id="IsAnyoneInjured"
          label="displaykey.NVV.Claim.SubView.LossDetailsProperty.Claim.FireDamage.IsAnyoneInjured"
          value="aClaim.PropertyFireDamage.IsAnyoneInjured"/>
        <BooleanRadioInput
          editable="true"
          id="SmokeDamageOnly"
          label="displaykey.NVV.Claim.SubView.LossDetailsProperty.Claim.FireDamage.SmokeDamageOnly"
          value="aClaim.PropertyFireDamage.SmokeDamageOnly"/>
        <BooleanRadioInput
          editable="true"
          id="IsHomeHabitable"
          label="displaykey.NVV.Claim.SubView.LossDetailsProperty.Claim.FireDamage.IsHomeHabitable"
          value="aClaim.PropertyFireDamage.IsHomeHabitable"/>
        <BooleanRadioInput
          editable="true"
          id="IsHomeSecure"
          label="displaykey.NVV.Claim.SubView.LossDetailsProperty.Claim.FireDamage.IsHomeSecure"
          value="aClaim.PropertyFireDamage.IsHomeSecure"/>
      </InputColumn>
    </DetailViewPanel>
  </PanelSet>
</PCF>
