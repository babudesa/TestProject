<?xml version="1.0"?>
<PCF
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="../../../../../../../pcf.xsd">
  <PanelSet
    id="ClaimSnapshotPolicyLocation600PanelSet">
    <Require
      name="Claim"
      type="Claim"/>
    <Require
      name="LocationBasedRU"
      type="snapshot.v600.LocationBasedRU"/>
    <DetailViewPanel
      id="ClaimSnapshotPolicyLocation600DV">
      <InputColumn>
        <Input
          id="Number"
          label="displaykey.Web.ClaimSnapshotPolicyLocationDV.Number"
          value="LocationBasedRU.RUNumber"/>
        <Input
          id="LocationNumber"
          label="displaykey.Web.ClaimSnapshotPolicyLocationDV.LocationNumber"
          value="LocationBasedRU.Property.LocationNumber"/>
        <InputSetRef
          def="AddressSnapshotInputSet(LocationBasedRU.Property.Address,null)"
          editable="false"/>
        <ListViewInput
          label="displaykey.Web.ClaimSnapshotPolicyLocationDV.Building">
          <Toolbar/>
          <ListViewPanel
            id="BuildingLV">
            <RowIterator
              editable="false"
              elementName="building"
              type="snapshot.v600.Building"
              value="getBuildingAsArray(LocationBasedRU)">
              <Row>
                <Cell
                  id="Number"
                  label="displaykey.Web.ClaimSnapshotPolicyLocationDV.Building.Number"
                  sortOrder="1"
                  value="building.BuildingNumber"/>
                <Cell
                  id="Notes"
                  label="displaykey.Web.ClaimSnapshotPolicyLocationDV.Building.Notes"
                  value="building.Notes"/>
              </Row>
            </RowIterator>
          </ListViewPanel>
        </ListViewInput>
      </InputColumn>
      <InputColumn>
        <Input
          id="Description"
          label="displaykey.Web.ClaimSnapshotPolicyLocationDV.Description"
          value="LocationBasedRU.Property.Address.Description"/>
        <Input
          id="Notes"
          label="displaykey.Web.ClaimSnapshotPolicyLocationDV.Notes"
          value="LocationBasedRU.Property.Notes"/>
        <InputDivider
          visible="LocationBasedRU.Property.Lienholders.length as Number &gt; 0"/>
        <ListViewInput
          label="displaykey.Web.ClaimSnapshotPolicyLocationDV.Lienholders"
          visible="LocationBasedRU.Property.Lienholders.length as Number &gt; 0">
          <Toolbar/>
          <ListViewPanel
            id="LienholdersLV">
            <RowIterator
              editable="false"
              elementName="PropertyOwner"
              type="snapshot.v600.PropertyOwner"
              value="LocationBasedRU.Property.Lienholders">
              <Row>
                <Cell
                  id="Lienholder"
                  label="displaykey.Web.ClaimSnapshotPolicyLocationDV.Lienholder"
                  value="util.Snapshot.renderValue(PropertyOwner.Lienholder)"/>
                <Cell
                  id="OwnerType"
                  label="displaykey.Web.ClaimSnapshotPolicyLocationDV.OwnerType"
                  value="util.Snapshot.renderValue(PropertyOwner.OwnerType)"/>
              </Row>
            </RowIterator>
          </ListViewPanel>
        </ListViewInput>
        <InputDivider
          visible="LocationBasedRU.Property.HighValueItems.length as Number &gt; 0"/>
        <ListViewInput
          label="displaykey.Web.ClaimSnapshotPolicyLocationDV.ListedItems"
          visible="LocationBasedRU.Property.HighValueItems.length as Number &gt; 0">
          <Toolbar/>
          <ListViewPanel
            id="ListedItemsLV">
            <RowIterator
              editable="false"
              elementName="PropertyItem"
              type="snapshot.v600.PropertyItem"
              value="LocationBasedRU.Property.HighValueItems">
              <Row>
                <Cell
                  id="ItemDescription"
                  label="displaykey.Web.ClaimSnapshotPolicyLocationDV.ItemDescription"
                  value="PropertyItem.Description"/>
                <Cell
                  formatType="currency"
                  id="ItemValue"
                  label="displaykey.Web.ClaimSnapshotPolicyLocationDV.ItemValue"
                  value="PropertyItem.AppraisedValue"/>
              </Row>
            </RowIterator>
          </ListViewPanel>
        </ListViewInput>
        <InputDivider
          visible="LocationBasedRU.ClassCode != null"/>
        <ListViewInput
          label="displaykey.Web.ClaimSnapshotPolicyLocationDV.ClassCode"
          visible="LocationBasedRU.ClassCode != null">
          <Toolbar/>
          <ListViewPanel
            id="ClassCodeLV">
            <RowIterator
              editable="false"
              elementName="classcode"
              type="snapshot.v600.ClassCode"
              value="getClassCodeAsArray(LocationBasedRU)">
              <Row>
                <Cell
                  id="Code"
                  label="displaykey.Web.ClaimSnapshotPolicyLocationDV.ClassCode.Code"
                  sortOrder="1"
                  value="classcode.Code"/>
                <Cell
                  id="Description"
                  label="displaykey.Web.ClaimSnapshotPolicyLocationDV.ClassCode.Description"
                  value="classcode.Description"/>
                <Cell
                  id="Comments"
                  label="displaykey.Web.ClaimSnapshotPolicyLocationDV.ClassCode.Comments"
                  value="classcode.Comments"/>
              </Row>
            </RowIterator>
          </ListViewPanel>
        </ListViewInput>
      </InputColumn>
    </DetailViewPanel>
    <ListDetailPanel
      id="ClaimSnapshotPolicyLocation600LDP"
      selectionName="SelectedCoverage"
      selectionType="snapshot.v600.Coverage">
      <PanelRef>
        <TitleBar
          title="displaykey.Web.ClaimSnapshotPolicyLocationDV.Coverages"/>
        <Toolbar/>
        <ListViewPanel
          id="CoveragesLV">
          <RowIterator
            editable="false"
            elementName="PropertyCoverage"
            type="snapshot.v600.PropertyCoverage"
            value="LocationBasedRU.Coverages">
            <Row>
              <Cell
                id="CoverageType"
                label="displaykey.Web.ClaimSnapshotPolicyLocationDV.CoverageType"
                sortOrder="1"
                value="PropertyCoverage.Type"/>
              <Cell
                formatType="currency"
                id="Deductible"
                label="displaykey.Web.ClaimSnapshotPolicyLocationDV.Deductible"
                value="PropertyCoverage.Deductible"/>
              <Cell
                formatType="currency"
                id="ExposureLimit"
                label="displaykey.Web.ClaimSnapshotPolicyLocationDV.ExposureLimit"
                value="PropertyCoverage.ExposureLimit"/>
              <Cell
                formatType="currency"
                id="IncidentLimit"
                label="displaykey.Web.ClaimSnapshotPolicyLocationDV.IncidentLimit"
                value="PropertyCoverage.IncidentLimit"/>
              <Cell
                formatType="currency"
                id="Coinsurance"
                label="displaykey.Web.ClaimSnapshotPolicyLocationDV.Coinsurance"
                value="PropertyCoverage.Coinsurance"/>
              <Cell
                id="CoverageBasis"
                label="displaykey.Web.ClaimSnapshotPolicyLocationDV.CoverageBasis"
                value="PropertyCoverage.CoverageBasis"/>
              <Cell
                id="LimitsIndicator"
                label="displaykey.Web.ClaimSnapshotPolicyLocationDV.LimitsIndicator"
                value="PropertyCoverage.LimitsIndicator"/>
              <Cell
                id="CoverageNotes"
                label="displaykey.Web.ClaimSnapshotPolicyLocationDV.CoverageNotes"
                value="PropertyCoverage.Notes"/>
            </Row>
          </RowIterator>
        </ListViewPanel>
      </PanelRef>
      <CardViewPanel>
        <Card
          id="CovTermsDetail"
          title="displaykey.Web.ClaimSnapshotPolicyLocationDV.CoverageDetail"
          visible="SelectedCoverage.CovTerms.length as Number &gt; 0">
          <DetailViewPanel
            compress="false">
            <InputFooterSection>
              <ListViewInput
                label="displaykey.Web.ClaimSnapshotPolicyLocationDV.CovTerms"
                labelAbove="true">
                <Toolbar/>
                <ListViewPanel
                  id="CovTermsLV">
                  <RowIterator
                    editable="false"
                    elementName="CovTerm"
                    value="SelectedCoverage.CovTerms">
                    <Row>
                      <Cell
                        id="Type"
                        label="displaykey.Web.ClaimSnapshotPolicyLocationDV.CovTerms.Type"
                        sortOrder="1"
                        value="CovTerm.Type"/>
                      <Cell
                        id="Value"
                        label="displaykey.Web.ClaimSnapshotPolicyLocationDV.CovTerms.Value"
                        value="CovTerm.Value"/>
                      <Cell
                        id="ValueType"
                        label="displaykey.Web.ClaimSnapshotPolicyLocationDV.CovTerms.ValueType"
                        value="CovTerm.ValueType"/>
                      <Cell
                        id="AggregationModel"
                        label="displaykey.Web.ClaimSnapshotPolicyLocationDV.CovTerms.AggregationModel"
                        value="CovTerm.AggregationModel"/>
                      <Cell
                        id="RestrictionModel"
                        label="displaykey.Web.ClaimSnapshotPolicyLocationDV.CovTerms.RestrictionModel"
                        value="CovTerm.RestrictionModel"/>
                    </Row>
                  </RowIterator>
                </ListViewPanel>
              </ListViewInput>
            </InputFooterSection>
          </DetailViewPanel>
        </Card>
      </CardViewPanel>
    </ListDetailPanel>
    <Code><![CDATA[function getBuildingAsArray(riskUnit : snapshot.v600.LocationBasedRU) : snapshot.v600.Building[] {
      if(riskUnit.Building != null) {
        return new snapshot.v600.Building[] {riskUnit.Building}
      } else {
        return new snapshot.v600.Building[] { }
      }
    }

    function getClassCodeAsArray(riskUnit : snapshot.v600.LocationBasedRU) : snapshot.v600.ClassCode[] {
      if(riskUnit.ClassCode != null) {
        return new snapshot.v600.ClassCode[] {riskUnit.ClassCode}
      } else {
        return new snapshot.v600.ClassCode[] { }
      }
    }]]></Code>
  </PanelSet>
</PCF>
