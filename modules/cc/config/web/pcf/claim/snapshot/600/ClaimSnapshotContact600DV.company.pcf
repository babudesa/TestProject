<?xml version="1.0"?>
<PCF
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="../../../../../../../pcf.xsd">
  <DetailViewPanel
    id="ClaimSnapshotContact600DV"
    mode="Company|CompanyVendor|MedicalCareOrg|AutoRepairShop|LawFirm|AutoTowingAgcy">
    <Require
      name="ClaimContactParam"
      type="Object"/>
    <InputHeaderSection>
      <Label
        label="displaykey.Web.ClaimSnapshotContactDV.Company.Claim"/>
      <ListViewInput
        labelAbove="true">
        <Toolbar/>
        <ListViewPanel
          id="ClaimRolesLV">
          <RowIterator
            editable="false"
            elementName="Role"
            value="asClaimContact().Roles">
            <Row>
              <Cell
                id="Role"
                label="displaykey.Web.ClaimSnapshotContactDV.Company.Role"
                value="Role.Role"/>
              <Cell
                id="Owner"
                label="displaykey.Web.ClaimSnapshotContactDV.Company.Owner"
                value="util.Snapshot.getRoleOwner(Role)"/>
              <Cell
                id="Active"
                label="displaykey.Web.ClaimSnapshotContactDV.Company.Active"
                value="util.Snapshot.renderValue(Role.Active)"/>
              <Cell
                id="Comments"
                label="displaykey.Web.ClaimSnapshotContactDV.Company.Comments"
                value="Role.Comments"/>
            </Row>
          </RowIterator>
        </ListViewPanel>
      </ListViewInput>
      <InputDivider/>
    </InputHeaderSection>
    <InputColumn>
      <Label
        label="displaykey.Web.ClaimSnapshotContactDV.Company.Organization"/>
      <Input
        id="Contact"
        label="displaykey.Web.ClaimSnapshotContactDV.Company.Contact"
        value="asClaimContact().Contact.DisplayName"/>
      <InputDivider/>
      <Label
        label="displaykey.Web.ClaimSnapshotContactDV.Company.Phone"/>
      <Input
        id="PrimaryPhone"
        label="displaykey.Web.ClaimSnapshotContactDV.Company.PrimaryPhone"
        value="asClaimContact().Contact.PrimaryPhone"/>
      <Input
        id="WorkPhone"
        label="displaykey.Web.ClaimSnapshotContactDV.Company.WorkPhone"
        value="asClaimContact().Contact.WorkPhone"/>
      <Input
        id="FaxPhone"
        label="displaykey.Web.ClaimSnapshotContactDV.Company.FaxPhone"
        value="asClaimContact().Contact.FaxPhone"/>
      <InputDivider/>
      <Label
        label="displaykey.Web.ClaimSnapshotContactDV.Company.Email"/>
      <Input
        id="EmailAddress1"
        label="displaykey.Web.ClaimSnapshotContactDV.Company.EmailAddress1"
        value="asClaimContact().Contact.EmailAddress1"/>
      <Input
        id="EmailAddress2"
        label="displaykey.Web.ClaimSnapshotContactDV.Company.EmailAddress2"
        value="asClaimContact().Contact.EmailAddress2"/>
      <InputDivider/>
    </InputColumn>
    <InputColumn>
      <Label
        label="displaykey.Web.ClaimSnapshotContactDV.Company.Primary"/>
      <InputSetRef
        def="AddressSnapshotInputSet(asClaimContact().Contact.PrimaryAddress,null)"
        editable="false"/>
      <Input
        id="AddressType"
        label="displaykey.Web.ClaimSnapshotContactDV.Company.AddressType"
        value="asClaimContact().Contact.PrimaryAddress.AddressType"/>
      <Input
        id="Description"
        label="displaykey.Web.ClaimSnapshotContactDV.Company.Description"
        value="asClaimContact().Contact.PrimaryAddress.Description"/>
      <Input
        id="ValidUntil"
        label="displaykey.Web.ClaimSnapshotContactDV.Company.ValidUntil"
        value="asClaimContact().Contact.PrimaryAddress.ValidUntil"/>
      <InputDivider/>
      <Label
        label="displaykey.Web.ClaimSnapshotContactDV.Company.Additional"/>
      <PrivacyInput
        encryptionExpression="gw.api.privacy.EncryptionMaskExpressions.maskTaxId(VALUE)"
        id="TaxID"
        label="displaykey.Web.ClaimSnapshotContactDV.Company.TaxID"
        value="asClaimContact().Contact.TaxID"/>
      <Input
        id="WithholdingRate"
        label="displaykey.Web.ClaimSnapshotContactDV.Company.WithholdingRate"
        value="asClaimContact().Contact.WithholdingRate"/>
      <Input
        id="TaxStatus"
        label="displaykey.Web.ClaimSnapshotContactDV.Company.TaxStatus"
        value="asClaimContact().Contact.TaxStatus"/>
      <Input
        id="PrimaryContact"
        label="displaykey.Web.ClaimSnapshotContactDV.Company.PrimaryContact"
        value="util.Snapshot.renderValue(asClaimContact().Contact.PrimaryContact)"/>
      <Input
        id="PreferredCurrency"
        label="displaykey.Web.ClaimSnapshotContactDV.Company.PreferredCurrency"
        value="asClaimContact().Contact.PreferredCurrency"/>
      <InputDivider/>
      <Label
        label="displaykey.Web.ClaimSnapshotContactDV.Company.Notes"/>
      <TextAreaInput
        id="Notes"
        numCols="30"
        numRows="3"
        value="asClaimContact().Contact.Notes"/>
      <InputDivider/>
    </InputColumn>
    <InputFooterSection>
      <InputDivider/>
      <ListViewInput
        boldLabel="true"
        def="ClaimSnapshotContactEFT600LV(asClaimContact().Contact)"
        label="displaykey.Web.ClaimSnapshotContactDV.Company.BankData"
        labelAbove="true">
        <Toolbar>
          <IteratorButtons/>
        </Toolbar>
      </ListViewInput>
    </InputFooterSection>
    <Code><![CDATA[
function asClaimContact() : snapshot.v600.ClaimContact {
        return ClaimContactParam as snapshot.v600.ClaimContact;
      }
    ]]></Code>
  </DetailViewPanel>
</PCF>
