<?xml version="1.0"?>
<PCF
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="../../../../../../../pcf.xsd">
  <ListViewPanel
    id="ClaimSnapshotPolicyStatCodes600LV">
    <Require
      name="Claim"
      type="Claim"/>
    <Require
      name="SnapshotParam"
      type="Object"/>
    <RowIterator
      editable="false"
      elementName="StatCode"
      value="(SnapshotParam as snapshot.v600.Claim).Policy.StatCodes">
      <Row>
        <Cell
          id="LineNumber"
          label="displaykey.Web.ClaimSnapshotPolicyStatCodesLV.LineNumber"
          value="StatCode.LineNumber"/>
        <Cell
          id="InsuranceLine"
          label="displaykey.Web.ClaimSnapshotPolicyStatCodesLV.InsuranceLine"
          value="StatCode.InsuranceLine"/>
        <Cell
          id="InsuranceSubline"
          label="displaykey.Web.ClaimSnapshotPolicyStatCodesLV.InsuranceSubline"
          value="StatCode.InsuranceSubline"/>
        <Cell
          id="LocationNumber"
          label="displaykey.Web.ClaimSnapshotPolicyStatCodesLV.LocationNumber"
          value="StatCode.LocationNumber"/>
        <Cell
          id="BuildingNumber"
          label="displaykey.Web.ClaimSnapshotPolicyStatCodesLV.BuildingNumber"
          value="StatCode.BuildingNumber"/>
        <Cell
          id="VehicleNumber"
          label="displaykey.Web.ClaimSnapshotPolicyStatCodesLV.VehicleNumber"
          value="StatCode.VehicleNumber"/>
        <Cell
          id="ClassCode"
          label="displaykey.Web.ClaimSnapshotPolicyStatCodesLV.ClassCode"
          value="StatCode.ClassCode"/>
        <Cell
          id="MajorPeril"
          label="displaykey.Web.ClaimSnapshotPolicyStatCodesLV.MajorPeril"
          value="StatCode.MajorPeril"/>
        <Cell
          id="State"
          label="displaykey.Web.ClaimSnapshotPolicyStatCodesLV.State"
          value="StatCode.State"
          visible="gw.api.address.CountryAddressFields.DefaultCountryHasStates"/>
        <Cell
          id="Notes"
          label="displaykey.Web.ClaimSnapshotPolicyStatCodesLV.Notes"
          value="StatCode.Notes"/>
      </Row>
    </RowIterator>
  </ListViewPanel>
</PCF>
