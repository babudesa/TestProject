<?xml version="1.0"?>
<PCF
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="../../../../../../../pcf.xsd">
  <DetailViewPanel
    id="ClaimSnapshotExposure600DV"
    mode="PIPDamages">
    <Require
      name="Claim"
      type="Claim"/>
    <Require
      name="Exposure"
      type="snapshot.v600.Exposure"/>
    <InputColumn>
      <Label
        label="displaykey.Web.ClaimSnapshotExposureDV.Pipdamages.General"/>
      <Input
        id="LossParty"
        label="displaykey.Web.ClaimSnapshotExposureDV.Pipdamages.LossParty"
        value="Exposure.LossParty"/>
      <Input
        id="PrimaryCoverage"
        label="displaykey.Web.ClaimSnapshotExposureDV.Pipdamages.PrimaryCoverage"
        value="Exposure.PrimaryCoverage"/>
      <Input
        id="CoverageSubType"
        label="displaykey.Web.ClaimSnapshotExposureDV.Pipdamages.CoverageSubType"
        value="Exposure.CoverageSubType"/>
      <Input
        id="Coverage"
        label="displaykey.Web.ClaimSnapshotExposureDV.Pipdamages.Coverage"
        value="Exposure.Coverage"/>
      <Input
        id="CreateTime"
        label="displaykey.Web.ClaimSnapshotExposureDV.Pipdamages.CreateTime"
        value="util.Snapshot.renderValue(Exposure.CreateTime)"/>
      <Input
        id="StatLine"
        label="displaykey.Web.ClaimSnapshotExposureDV.Pipdamages.StatLine"
        value="util.Snapshot.renderValue(Exposure.StatLine)"/>
      <InputDivider/>
      <Label
        label="displaykey.Web.ClaimSnapshotExposureDV.Pipdamages.Claimant"/>
      <Input
        id="Claimant"
        label="displaykey.Web.ClaimSnapshotExposureDV.Pipdamages.Claimant"
        value="util.Snapshot.renderValue(Exposure.Claimant)"/>
      <Input
        id="ClaimantType"
        label="displaykey.Web.ClaimSnapshotExposureDV.Pipdamages.ClaimantType"
        value="Exposure.ClaimantType"/>
      <BooleanRadioInput
        falseLabel="displaykey.Web.ClaimSnapshotExposureDV.Pipdamages.ContactProhibited"
        id="ContactProhibited"
        label="displaykey.Web.ClaimSnapshotExposureDV.Pipdamages.ContactProhibited_1"
        trueLabel="displaykey.Web.ClaimSnapshotExposureDV.Pipdamages.ContactProhibited_2"
        value="util.Snapshot.getClaimant(Exposure.Claim, Exposure)[&quot;ContactProhibited&quot;]"/>
      <Input
        id="PrimaryPhone"
        label="displaykey.Web.ClaimSnapshotExposureDV.Pipdamages.PrimaryPhone"
        value="util.Snapshot.getPrimaryPhone(Exposure.Claimant)"/>
      <Input
        id="PrimaryAddress"
        label="displaykey.Web.ClaimSnapshotExposureDV.Pipdamages.PrimaryAddress"
        value="Exposure.Claimant.PrimaryAddress.DisplayName"/>
      <InputDivider/>
      <Label
        label="displaykey.Web.ClaimSnapshotExposureDV.Pipdamages.Claimant_2"/>
      <Input
        id="Occupation"
        label="displaykey.Web.ClaimSnapshotExposureDV.Pipdamages.Occupation"
        value="Exposure.Claimant.Occupation"/>
      <Input
        id="ContactCompany"
        label="displaykey.Web.ClaimSnapshotExposureDV.Pipdamages.ContactCompany"
        value="Exposure.Claimant.ContactCompany.DisplayName"/>
      <Input
        id="PrimaryContact"
        label="displaykey.Web.ClaimSnapshotExposureDV.Pipdamages.PrimaryContact"
        value="Exposure.Claimant.ContactCompany.PrimaryContact.DisplayName"/>
      <Input
        id="HireDate"
        label="displaykey.Web.ClaimSnapshotExposureDV.Pipdamages.HireDate"
        value="util.Snapshot.renderValue(Exposure.PriorEmpData.HireDate)"/>
      <Input
        id="EmploymentStatus"
        label="displaykey.Web.ClaimSnapshotExposureDV.Pipdamages.EmploymentStatus"
        value="Exposure.PriorEmpData.EmploymentStatus"/>
      <Input
        id="NumHoursWorked"
        label="displaykey.Web.ClaimSnapshotExposureDV.Pipdamages.NumHoursWorked"
        value="Exposure.PriorEmpData.NumHoursWorked"/>
      <Input
        id="NumDaysWorked"
        label="displaykey.Web.ClaimSnapshotExposureDV.Pipdamages.NumDaysWorked"
        value="Exposure.PriorEmpData.NumDaysWorked"/>
      <Input
        id="WageAmount"
        label="displaykey.Web.ClaimSnapshotExposureDV.Pipdamages.WageAmount"
        value="Exposure.PriorEmpData.WageAmount"/>
      <Input
        id="OvertimeRate"
        label="displaykey.Web.ClaimSnapshotExposureDV.Pipdamages.OvertimeRate"
        value="Exposure.PriorEmpData.OvertimeRate"/>
      <Input
        id="LastYearIncome"
        label="displaykey.Web.ClaimSnapshotExposureDV.Pipdamages.LastYearIncome"
        value="Exposure.PriorEmpData.LastYearIncome"/>
      <Input
        id="PayPeriod"
        label="displaykey.Web.ClaimSnapshotExposureDV.Pipdamages.PayPeriod"
        value="Exposure.PriorEmpData.PayPeriod"/>
      <BooleanRadioInput
        falseLabel="displaykey.Web.ClaimSnapshotExposureDV.Pipdamages.ContactProhibited"
        id="WagePaymentCont"
        label="displaykey.Web.ClaimSnapshotExposureDV.Pipdamages.WagePaymentCont_1"
        trueLabel="displaykey.Web.ClaimSnapshotExposureDV.Pipdamages.ContactProhibited_2"
        value="Exposure.PriorEmpData.WagePaymentCont"/>
      <Input
        id="InjuryStartTime"
        label="displaykey.Web.ClaimSnapshotExposureDV.Pipdamages.InjuryStartTime"
        value="Exposure.PriorEmpData.InjuryStartTime"/>
      <Input
        id="DaysWorkedWeek"
        label="displaykey.Web.ClaimSnapshotExposureDV.Pipdamages.DaysWorkedWeek"
        value="Exposure.PriorEmpData.DaysWorkedWeek"/>
      <InputDivider/>
      <Label
        label="displaykey.Web.ClaimSnapshotExposureDV.Pipdamages.Work"/>
      <ListViewInput
        labelAbove="true">
        <Toolbar/>
        <ListViewPanel
          id="WorkStatusChangesLV">
          <RowIterator
            editable="false"
            elementName="WorkStatusChange"
            value="Exposure.PriorEmpData.WorkStatusChanges">
            <Row>
              <Cell
                id="StatusDate"
                label="displaykey.Web.ClaimSnapshotExposureDV.Pipdamages.StatusDate"
                value="util.Snapshot.renderValue(WorkStatusChange.StatusDate)"
                wrap="false"/>
              <Cell
                id="Status"
                label="displaykey.Web.ClaimSnapshotExposureDV.Pipdamages.Status"
                value="WorkStatusChange.Status"
                wrap="false"/>
              <Cell
                id="Comments"
                label="displaykey.Web.ClaimSnapshotExposureDV.Pipdamages.Comments"
                value="WorkStatusChange.Comments"/>
            </Row>
          </RowIterator>
        </ListViewPanel>
      </ListViewInput>
      <InputDivider/>
    </InputColumn>
    <InputColumn>
      <Label
        label="displaykey.Web.ClaimSnapshotExposureDV.Pipdamages.Coding"
        visible="gw.api.address.CountryAddressFields.DefaultCountryHasStates"/>
      <Input
        id="JurisdictionState"
        label="displaykey.Web.ClaimSnapshotExposureDV.Pipdamages.JurisdictionState"
        value="Exposure.JurisdictionState"
        visible="gw.api.address.CountryAddressFields.DefaultCountryHasStates"/>
      <InputDivider
        visible="gw.api.address.CountryAddressFields.DefaultCountryHasStates"/>
      <Label
        label="displaykey.Web.ClaimSnapshotExposureDV.Pipdamages.Other"/>
      <BooleanRadioInput
        falseLabel="displaykey.Web.ClaimSnapshotExposureDV.Pipdamages.ContactProhibited"
        id="OtherCoverage"
        label="displaykey.Web.ClaimSnapshotExposureDV.Pipdamages.OtherCoverage_1"
        trueLabel="displaykey.Web.ClaimSnapshotExposureDV.Pipdamages.ContactProhibited_2"
        value="Exposure.OtherCoverage"/>
      <ListViewInput
        label="displaykey.Web.ClaimSnapshotExposureDV.Pipdamages.Details">
        <Toolbar/>
        <ListViewPanel
          id="OtherCoverageDetLV">
          <RowIterator
            editable="false"
            elementName="OtherCoverageDet"
            value="Exposure.OtherCoverageDet">
            <Row>
              <Cell
                id="Insurer"
                label="displaykey.Web.ClaimSnapshotExposureDV.Pipdamages.Insurer"
                value="OtherCoverageDet.Insurer"/>
              <Cell
                id="PolicyNumber"
                label="displaykey.Web.ClaimSnapshotExposureDV.Pipdamages.PolicyNumber"
                value="OtherCoverageDet.PolicyNumber"/>
              <Cell
                id="ContactName"
                label="displaykey.Web.ClaimSnapshotExposureDV.Pipdamages.ContactName"
                value="OtherCoverageDet.ContactName"/>
              <Cell
                id="ContactPhone"
                label="displaykey.Web.ClaimSnapshotExposureDV.Pipdamages.ContactPhone"
                value="OtherCoverageDet.ContactPhone"/>
              <Cell
                id="Notes"
                label="displaykey.Web.ClaimSnapshotExposureDV.Pipdamages.Notes"
                value="OtherCoverageDet.Notes"/>
            </Row>
          </RowIterator>
        </ListViewPanel>
      </ListViewInput>
      <InputDivider/>
    </InputColumn>
  </DetailViewPanel>
</PCF>
