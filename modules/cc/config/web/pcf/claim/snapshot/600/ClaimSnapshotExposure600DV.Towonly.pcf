<?xml version="1.0"?>
<PCF
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="../../../../../../../pcf.xsd">
  <DetailViewPanel
    id="ClaimSnapshotExposure600DV"
    mode="TowOnly">
    <Require
      name="Claim"
      type="Claim"/>
    <Require
      name="Exposure"
      type="snapshot.v600.Exposure"/>
    <InputColumn>
      <Label
        label="displaykey.Web.ClaimSnapshotExposureDV.Towonly.Overall"/>
      <Input
        id="LossParty"
        label="displaykey.Web.ClaimSnapshotExposureDV.Towonly.LossParty"
        value="Exposure.LossParty"/>
      <Input
        id="PrimaryCoverage"
        label="displaykey.Web.ClaimSnapshotExposureDV.Towonly.PrimaryCoverage"
        value="Exposure.PrimaryCoverage"/>
      <Input
        id="CoverageSubType"
        label="displaykey.Web.ClaimSnapshotExposureDV.Towonly.CoverageSubType"
        value="Exposure.CoverageSubType"/>
      <Input
        id="Coverage"
        label="displaykey.Web.ClaimSnapshotExposureDV.Towonly.Coverage"
        value="util.Snapshot.renderValue(Exposure.Coverage)"/>
      <Input
        id="CreateTime"
        label="displaykey.Web.ClaimSnapshotExposureDV.Towonly.CreateTime"
        value="util.Snapshot.renderValue(Exposure.CreateTime)"/>
      <Input
        id="StatLine"
        label="displaykey.Web.ClaimSnapshotExposureDV.Towonly.StatLine"
        value="Exposure.StatLine"/>
      <InputDivider/>
      <Label
        label="displaykey.Web.ClaimSnapshotExposureDV.Towonly.Claimant"/>
      <Input
        id="Claimant"
        label="displaykey.Web.ClaimSnapshotExposureDV.Towonly.Claimant"
        value="util.Snapshot.renderValue(Exposure.Claimant)"/>
      <Input
        id="ClaimantType"
        label="displaykey.Web.ClaimSnapshotExposureDV.Towonly.ClaimantType"
        value="Exposure.ClaimantType"/>
      <BooleanRadioInput
        falseLabel="displaykey.Web.ClaimSnapshotExposureDV.Towonly.ContactProhibited"
        id="ContactProhibited"
        label="displaykey.Web.ClaimSnapshotExposureDV.Towonly.ContactProhibited_1"
        trueLabel="displaykey.Web.ClaimSnapshotExposureDV.Towonly.ContactProhibited_2"
        value="util.Snapshot.getClaimant(Exposure.Claim, Exposure)[&quot;ContactProhibited&quot;]"/>
      <Input
        id="PrimaryPhone"
        label="displaykey.Web.ClaimSnapshotExposureDV.Towonly.PrimaryPhone"
        value="util.Snapshot.getPrimaryPhone(Exposure.Claimant)"/>
      <Input
        id="ClaimantAddress"
        label="displaykey.Web.ClaimSnapshotExposureDV.Towonly.ClaimantAddress"
        value="Exposure.Claimant.PrimaryAddress.DisplayName"/>
      <InputDivider/>
      <Label
        label="displaykey.Web.ClaimSnapshotExposureDV.Towonly.IncidentOverview"/>
      <Input
        action="ClaimSnapshotVehicleIncidentPopup.push(Claim, Exposure.VehicleIncident)"
        id="Vehicle_Incident"
        label="displaykey.Web.ClaimSnapshotExposureDV.Towonly.VehicleIncident"
        value="Exposure.VehicleIncident.DisplayName"/>
      <Input
        id="Incident_Driver"
        label="displaykey.Web.ClaimSnapshotExposureDV.Towonly.Incident.Driver"
        value="Exposure.VehicleIncident.Driver.DisplayName"/>
      <Input
        id="Incident_Description"
        label="displaykey.Web.ClaimSnapshotExposureDV.Towonly.Incident.Description"
        value="Exposure.VehicleIncident.Description"/>
      <BooleanRadioInput
        id="Incident_VehicleOperable"
        label="displaykey.Web.ClaimSnapshotExposureDV.Towonly.Incident.VehicleOperable"
        value="Exposure.VehicleIncident.VehicleOperable"/>
      <Input
        id="Incident_LossEstimate"
        label="displaykey.Web.ClaimSnapshotExposureDV.Towonly.Incident.LossEstimate"
        value="Exposure.VehicleIncident.LossEstimate"/>
      <InputDivider/>
    </InputColumn>
    <InputColumn>
      <Label
        label="displaykey.Web.ClaimSnapshotExposureDV.Towonly.Coding"
        visible="gw.api.address.CountryAddressFields.DefaultCountryHasStates"/>
      <Input
        id="JurisdictionState"
        label="displaykey.Web.ClaimSnapshotExposureDV.Towonly.JurisdictionState"
        value="Exposure.JurisdictionState"
        visible="gw.api.address.CountryAddressFields.DefaultCountryHasStates"/>
      <InputDivider
        visible="gw.api.address.CountryAddressFields.DefaultCountryHasStates"/>
      <Label
        label="displaykey.Web.ClaimSnapshotExposureDV.Towonly.Other"/>
      <BooleanRadioInput
        falseLabel="displaykey.Web.ClaimSnapshotExposureDV.Towonly.ContactProhibited"
        id="OtherCoverage"
        label="displaykey.Web.ClaimSnapshotExposureDV.Towonly.OtherCoverage_1"
        trueLabel="displaykey.Web.ClaimSnapshotExposureDV.Towonly.ContactProhibited_2"
        value="Exposure.OtherCoverage"/>
      <ListViewInput
        label="displaykey.Web.ClaimSnapshotExposureDV.Towonly.Details">
        <Toolbar/>
        <ListViewPanel
          id="OtherCoverageDetLV">
          <RowIterator
            editable="false"
            elementName="OtherCoverageDet"
            value="Exposure.OtherCoverageDet">
            <Row>
              <Cell
                id="Insurer"
                label="displaykey.Web.ClaimSnapshotExposureDV.Towonly.Insurer"
                value="OtherCoverageDet.Insurer"/>
              <Cell
                id="PolicyNumber"
                label="displaykey.Web.ClaimSnapshotExposureDV.Towonly.PolicyNumber"
                value="OtherCoverageDet.PolicyNumber"/>
              <Cell
                id="ContactName"
                label="displaykey.Web.ClaimSnapshotExposureDV.Towonly.ContactName"
                value="OtherCoverageDet.ContactName"/>
              <Cell
                id="ContactPhone"
                label="displaykey.Web.ClaimSnapshotExposureDV.Towonly.ContactPhone"
                value="OtherCoverageDet.ContactPhone"/>
              <Cell
                id="Notes"
                label="displaykey.Web.ClaimSnapshotExposureDV.Towonly.Notes"
                value="OtherCoverageDet.Notes"/>
            </Row>
          </RowIterator>
        </ListViewPanel>
      </ListViewInput>
      <InputDivider/>
    </InputColumn>
  </DetailViewPanel>
</PCF>
