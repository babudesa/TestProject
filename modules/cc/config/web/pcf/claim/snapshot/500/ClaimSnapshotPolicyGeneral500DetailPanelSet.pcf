<?xml version="1.0"?>
<PCF
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="../../../../../../../pcf.xsd">
  <PanelSet
    id="ClaimSnapshotPolicyGeneral500DetailPanelSet">
    <Require
      name="Claim"
      type="Claim"/>
    <Require
      name="SnapshotParam"
      type="Object"/>
    <DetailViewPanel
      id="ClaimSnapshotPolicyGeneral500DV">
      <InputColumn>
        <Label
          label="displaykey.Web.ClaimSnapshotPolicyGeneral500DV.Basic"/>
        <Input
          id="PolicyNumber"
          label="displaykey.Web.ClaimSnapshotPolicyGeneral500DV.PolicyNumber"
          value="asPolicy().PolicyNumber"/>
        <Input
          id="PolicyType"
          label="displaykey.Web.ClaimSnapshotPolicyGeneral500DV.Type"
          value="asPolicy().PolicyType"/>
        <Input
          id="EffectiveDate"
          label="displaykey.Web.ClaimSnapshotPolicyGeneral500DV.EffectiveDate"
          value="util.Snapshot.renderValue(asPolicy().EffectiveDate)"/>
        <Input
          id="ExpirationDate"
          label="displaykey.Web.ClaimSnapshotPolicyGeneral500DV.ExpirationDate"
          value="util.Snapshot.renderValue(asPolicy().ExpirationDate)"/>
        <Input
          id="CancellationDate"
          label="displaykey.Web.ClaimSnapshotPolicyGeneral500DV.CancellationDate"
          value="util.Snapshot.renderValue(asPolicy().CancellationDate)"/>
        <Input
          id="OrigEffectiveDate"
          label="displaykey.Web.ClaimSnapshotPolicyGeneral500DV.OrigEffectiveDate"
          value="util.Snapshot.renderValue(asPolicy().OrigEffectiveDate)"/>
        <Input
          id="Status"
          label="displaykey.Web.ClaimSnapshotPolicyGeneral500DV.Status"
          value="asPolicy().Status"/>
        <InputDivider/>
        <Label
          label="displaykey.Web.ClaimSnapshotPolicyGeneral500DV.Insured"/>
        <Input
          id="Insured"
          label="displaykey.Web.ClaimSnapshotPolicyGeneral500DV.Insured_1"
          value="asPolicy().Insured.DisplayName"/>
        <TextAreaInput
          id="Address"
          label="displaykey.Web.ClaimSnapshotPolicyGeneral500DV.Address"
          numCols="30"
          numRows="3"
          value="asPolicy().Insured.PrimaryAddress.DisplayName"/>
        <Input
          id="Account"
          label="displaykey.Web.ClaimSnapshotPolicyGeneral500DV.Account"
          value="asPolicy().Account"/>
      </InputColumn>
      <InputColumn>
        <Label
          label="displaykey.Web.ClaimSnapshotPolicyGeneral500DV.Additional"/>
        <ListViewInput
          labelAbove="true">
          <Toolbar/>
          <ListViewPanel
            id="AdditionalInsuredLV">
            <RowIterator
              editable="false"
              elementName="ClaimContactRole"
              type="snapshot.v500.ClaimContactRole"
              value="util.Snapshot.getClaimContactRolesByRole(SnapshotParam as snapshot.v500.Claim, ContactRole.TC_COVEREDPARTY.Code)">
              <Row>
                <Cell
                  id="Name"
                  label="displaykey.Web.ClaimSnapshotPolicyGeneral500DV.Insured_1"
                  value="ClaimContactRole.ClaimContact.Contact.DisplayName"/>
                <Cell
                  id="Type"
                  label="displaykey.Web.ClaimSnapshotPolicyGeneral500DV.Type"
                  value="ClaimContactRole.CoveredPartyType"/>
              </Row>
            </RowIterator>
          </ListViewPanel>
        </ListViewInput>
        <InputDivider/>
        <Label
          label="displaykey.Web.ClaimSnapshotPolicyGeneral500DV.Excluded"/>
        <ListViewInput
          labelAbove="true">
          <Toolbar/>
          <ListViewPanel
            id="ExcludedPartiesLV">
            <RowIterator
              editable="false"
              elementName="ClaimContactRole"
              type="snapshot.v500.ClaimContactRole"
              value="util.Snapshot.getClaimContactRolesByRole(SnapshotParam as snapshot.v500.Claim, ContactRole.TC_EXCLUDEDPARTY.Code)">
              <Row>
                <Cell
                  id="Name"
                  label="displaykey.Web.ClaimSnapshotPolicyGeneral500DV.Insured_1"
                  value="ClaimContactRole.ClaimContact.Contact.DisplayName"/>
              </Row>
            </RowIterator>
          </ListViewPanel>
        </ListViewInput>
        <InputDivider/>
        <Label
          label="displaykey.Web.ClaimSnapshotPolicyGeneral500DV.Agent"/>
        <Input
          id="Agent"
          label="displaykey.Web.ClaimSnapshotPolicyGeneral500DV.Insured_1"
          value="asPolicy().Agent.DisplayName"/>
        <Input
          id="ProducerCode"
          label="displaykey.Web.ClaimSnapshotPolicyGeneral500DV.ProducerCode"
          value="asPolicy().ProducerCode"/>
        <InputDivider/>
        <Label
          label="displaykey.Web.ClaimSnapshotPolicyGeneral500DV.Underwriting"/>
        <Input
          id="Underwriter"
          label="displaykey.Web.ClaimSnapshotPolicyGeneral500DV.Underwriter"
          value="asPolicy().Underwriter.DisplayName"/>
        <Input
          id="UnderwritingCo"
          label="displaykey.Web.ClaimSnapshotPolicyGeneral500DV.UnderwritingCo"
          value="asPolicy().UnderwritingCo"/>
        <Input
          id="UnderwritingGroup"
          label="displaykey.Web.ClaimSnapshotPolicyGeneral500DV.UnderwritingGroup"
          value="asPolicy().UnderwritingGroup"/>
        <InputDivider/>
        <Label
          label="displaykey.Web.ClaimSnapshotPolicyGeneral500DV.Other"/>
        <BooleanRadioInput
          falseLabel="displaykey.Web.ClaimSnapshotPolicyGeneral500DV.Verified"
          id="Verified"
          label="displaykey.Web.ClaimSnapshotPolicyGeneral500DV.Verified_1"
          trueLabel="displaykey.Web.ClaimSnapshotPolicyGeneral500DV.Verified_2"
          value="asPolicy().Verified"/>
        <BooleanRadioInput
          falseLabel="displaykey.Web.ClaimSnapshotPolicyGeneral500DV.Verified"
          id="ForeignCoverage"
          label="displaykey.Web.ClaimSnapshotPolicyGeneral500DV.ForeignCoverage_1"
          trueLabel="displaykey.Web.ClaimSnapshotPolicyGeneral500DV.Verified_2"
          value="asPolicy().ForeignCoverage"/>
        <Input
          id="FinancialInterests"
          label="displaykey.Web.ClaimSnapshotPolicyGeneral500DV.FinancialInterests"
          value="asPolicy().FinancialInterests"/>
        <Input
          id="Notes"
          label="displaykey.Web.ClaimSnapshotPolicyGeneral500DV.Notes"
          value="asPolicy().Notes"/>
        <InputSet
          visible="&quot;Workers' Comp&quot; == (SnapshotParam as snapshot.v400.Claim).LossType">
          <InputDivider/>
          <Label
            label="displaykey.Web.ClaimSnapshotPolicyGeneral500DV.Workers"/>
          <Input
            id="InsuredSICCode"
            label="displaykey.Web.ClaimSnapshotPolicyGeneral500DV.InsuredSICCode"
            value="asPolicy().InsuredSICCode"/>
          <Input
            id="PolicyRatingPlan"
            label="displaykey.Web.ClaimSnapshotPolicyGeneral500DV.PolicyRatingPlan"
            value="asPolicy().PolicyRatingPlan"/>
          <Input
            id="WCStates"
            label="displaykey.Web.ClaimSnapshotPolicyGeneral500DV.WCStates"
            value="asPolicy().WCStates"/>
          <Input
            id="WCOtherStates"
            label="displaykey.Web.ClaimSnapshotPolicyGeneral500DV.WCOtherStates"
            value="asPolicy().WCOtherStates"/>
        </InputSet>
      </InputColumn>
    </DetailViewPanel>
    <ListDetailPanel
      id="ClaimSnapshotPolicyGeneral500LDP"
      selectionName="SelectedCoverage"
      selectionType="snapshot.v500.Coverage">
      <PanelRef>
        <TitleBar
          title="displaykey.Web.ClaimSnapshotPolicyGeneral500DV.Policylevel"/>
        <Toolbar/>
        <ListViewPanel
          id="PolicyCoveragesLV">
          <RowIterator
            editable="false"
            elementName="Coverage"
            value="asPolicy().Coverages">
            <Row>
              <Cell
                id="Type"
                label="displaykey.Web.ClaimSnapshotPolicyGeneral500DV.Type"
                value="Coverage.Type"/>
              <Cell
                id="Deductible"
                label="displaykey.Web.ClaimSnapshotPolicyGeneral500DV.Deductible"
                value="Coverage.Deductible"/>
              <Cell
                id="ExposureLimit"
                label="displaykey.Web.ClaimSnapshotPolicyGeneral500DV.ExposureLimit"
                value="Coverage.ExposureLimit"/>
              <Cell
                id="IncidentLimit"
                label="displaykey.Web.ClaimSnapshotPolicyGeneral500DV.IncidentLimit"
                value="Coverage.IncidentLimit"/>
              <Cell
                id="Notes"
                label="displaykey.Web.ClaimSnapshotPolicyGeneral500DV.Notes"
                value="Coverage.Notes"/>
            </Row>
          </RowIterator>
        </ListViewPanel>
      </PanelRef>
      <CardViewPanel>
        <Card
          id="CovTermsDetail"
          title="displaykey.Web.ClaimSnapshotPolicyGeneral500DV.CoverageDetail"
          visible="SelectedCoverage.CovTerms.length as Number &gt; 0">
          <DetailViewPanel
            compress="false">
            <InputFooterSection>
              <ListViewInput
                label="displaykey.Web.ClaimSnapshotPolicyGeneral500DV.CovTerms"
                labelAbove="true">
                <Toolbar/>
                <ListViewPanel
                  id="CovTermsLV">
                  <RowIterator
                    editable="false"
                    elementName="CovTerm"
                    value="SelectedCoverage.CovTerms">
                    <Row>
                      <Cell
                        id="Type"
                        label="displaykey.Web.ClaimSnapshotPolicyGeneral500DV.CovTerms.Type"
                        sortOrder="1"
                        value="CovTerm.Type"/>
                      <Cell
                        id="Value"
                        label="displaykey.Web.ClaimSnapshotPolicyGeneral500DV.CovTerms.Value"
                        value="CovTerm.Value"/>
                      <Cell
                        id="ValueType"
                        label="displaykey.Web.ClaimSnapshotPolicyGeneral500DV.CovTerms.ValueType"
                        value="CovTerm.ValueType"/>
                      <Cell
                        id="AggregationModel"
                        label="displaykey.Web.ClaimSnapshotPolicyGeneral500DV.CovTerms.AggregationModel"
                        value="CovTerm.AggregationModel"/>
                      <Cell
                        id="RestrictionModel"
                        label="displaykey.Web.ClaimSnapshotPolicyGeneral500DV.CovTerms.RestrictionModel"
                        value="CovTerm.RestrictionModel"/>
                    </Row>
                  </RowIterator>
                </ListViewPanel>
              </ListViewInput>
            </InputFooterSection>
          </DetailViewPanel>
        </Card>
      </CardViewPanel>
    </ListDetailPanel>
    <Code><![CDATA[
      function asPolicy() : snapshot.v500.Policy {
        return (SnapshotParam as snapshot.v500.Claim).Policy;
      }
    ]]></Code>
  </PanelSet>
</PCF>
