<?xml version="1.0"?>
<PCF
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="../../../../../../../pcf.xsd">
  <DetailViewPanel
    id="ClaimSnapshotGeneral310DV"
    mode="auto">
    <Require
      name="Claim"
      type="Claim"/>
    <Require
      name="Snapshot"
      type="snapshot.v310.Claim"/>
    <InputColumn>
      <Label
        label="displaykey.Web.ClaimSnapshotGeneral310DV.Auto.General"/>
      <Input
        id="LossType"
        label="displaykey.Web.ClaimSnapshotGeneral310DV.Auto.LossType"
        value="Snapshot.LossType"/>
      <Input
        id="LOBCode"
        label="displaykey.Web.ClaimSnapshotGeneral310DV.Auto.LOBCode"
        value="Snapshot.LOBCode"/>
      <Input
        id="Contact"
        label="displaykey.Web.ClaimSnapshotGeneral310DV.Auto.Contact"
        value="Snapshot.AssignedUser.Contact.DisplayName"/>
      <Input
        id="Catastrophe"
        label="displaykey.Web.ClaimSnapshotGeneral310DV.Auto.Catastrophe"
        value="Snapshot.Catastrophe.DisplayName"/>
      <Input
        id="Segment"
        label="displaykey.Web.ClaimSnapshotGeneral310DV.Auto.Segment"
        value="Snapshot.Segment"/>
      <Input
        id="Strategy"
        label="displaykey.Web.ClaimSnapshotGeneral310DV.Auto.Strategy"
        value="Snapshot.Strategy"/>
      <Input
        id="PermissionRequired"
        label="displaykey.Web.ClaimSnapshotGeneral310DV.Auto.PermissionRequired"
        value="Snapshot.PermissionRequired"/>
      <InputDivider/>
      <Label
        label="displaykey.Web.ClaimSnapshotGeneral310DV.Auto.Description"/>
      <TextAreaInput
        id="Description"
        numCols="30"
        numRows="3"
        value="Snapshot.Description"/>
      <InputDivider/>
      <Label
        label="displaykey.Web.ClaimSnapshotGeneral310DV.Auto.Loss"/>
      <Input
        id="LossCause"
        label="displaykey.Web.ClaimSnapshotGeneral310DV.Auto.LossCause"
        value="Snapshot.LossCause"/>
      <Input
        id="LossDate"
        label="displaykey.Web.ClaimSnapshotGeneral310DV.Auto.LossDate"
        value="util.Snapshot.renderValue(Snapshot.LossDate)"/>
      <InputDivider/>
      <Label
        label="displaykey.Web.ClaimSnapshotGeneral310DV.Auto.Loss_1"/>
      <Input
        id="LossLocation"
        label="displaykey.Web.ClaimSnapshotGeneral310DV.Auto.LossLocation"
        value="Snapshot.LossLocation.DisplayName"/>
      <Input
        id="County"
        label="displaykey.Web.ClaimSnapshotGeneral310DV.Auto.County"
        value="Snapshot.LossLocation.County"/>
      <Input
        id="Country"
        label="displaykey.Web.ClaimSnapshotGeneral310DV.Auto.Country"
        value="Snapshot.LossLocation.Country"/>
      <Input
        id="LossLocationCode"
        label="displaykey.Web.ClaimSnapshotGeneral310DV.Auto.LossLocationCode"
        value="Snapshot.LossLocationCode"/>
      <Input
        id="JurisdictionState"
        label="displaykey.Web.ClaimSnapshotGeneral310DV.Auto.JurisdictionState"
        value="Snapshot.JurisdictionState"/>
      <InputDivider/>
      <Label
        label="displaykey.Web.ClaimSnapshotGeneral310DV.Auto.Notification"/>
      <Input
        id="ReportedDate"
        label="displaykey.Web.ClaimSnapshotGeneral310DV.Auto.ReportedDate"
        value="util.Snapshot.renderValue(Snapshot.ReportedDate)"/>
      <BooleanRadioInput
        falseLabel="displaykey.Web.ClaimSnapshotGeneral310DV.Auto.FirstNoticeSuit"
        id="FirstNoticeSuit"
        label="displaykey.Web.ClaimSnapshotGeneral310DV.Auto.FirstNoticeSuit_1"
        trueLabel="displaykey.Web.ClaimSnapshotGeneral310DV.Auto.FirstNoticeSuit_2"
        value="Snapshot.FirstNoticeSuit"/>
      <Input
        id="HowReported"
        label="displaykey.Web.ClaimSnapshotGeneral310DV.Auto.HowReported"
        value="Snapshot.HowReported"/>
      <Input
        id="Reporter"
        label="displaykey.Web.ClaimSnapshotGeneral310DV.Auto.Reporter"
        value="Snapshot.Reporter.DisplayName"/>
      <Input
        id="DriverRelation"
        label="displaykey.Web.ClaimSnapshotGeneral310DV.Auto.DriverRelation"
        value="Snapshot.DriverRelation"/>
      <Input
        id="MainContact"
        label="displaykey.Web.ClaimSnapshotGeneral310DV.Auto.MainContact"
        value="Snapshot.MainContact.DisplayName"/>
      <Input
        id="MainContactType"
        label="displaykey.Web.ClaimSnapshotGeneral310DV.Auto.DriverRelation"
        value="Snapshot.MainContactType"/>
      <Input
        id="DateRptdToAgent"
        label="displaykey.Web.ClaimSnapshotGeneral310DV.Auto.DateRptdToAgent"
        value="util.Snapshot.renderValue(Snapshot.DateRptdToAgent)"/>
      <Input
        id="Fault"
        label="displaykey.Web.ClaimSnapshotGeneral310DV.Auto.Fault"
        value="Snapshot.Fault"/>
    </InputColumn>
    <InputColumn>
      <Label
        label="displaykey.Web.ClaimSnapshotGeneral310DV.Auto.Insured"/>
      <Input
        id="Vehicle"
        label="displaykey.Web.ClaimSnapshotGeneral310DV.Auto.Vehicle"
        value="Snapshot.Vehicle.DisplayName"/>
      <Input
        id="Style"
        label="displaykey.Web.ClaimSnapshotGeneral310DV.Auto.Style"
        value="Snapshot.Vehicle.Style"/>
      <Input
        id="Color"
        label="displaykey.Web.ClaimSnapshotGeneral310DV.Auto.Color"
        value="Snapshot.Vehicle.Color"/>
      <Input
        id="Vin"
        label="displaykey.Web.ClaimSnapshotGeneral310DV.Auto.Vin"
        value="Snapshot.Vehicle.Vin"/>
      <Input
        id="State"
        label="displaykey.Web.ClaimSnapshotGeneral310DV.Auto.State"
        value="Snapshot.Vehicle.State"/>
      <Input
        id="LicensePlate"
        label="displaykey.Web.ClaimSnapshotGeneral310DV.Auto.LicensePlate"
        value="Snapshot.Vehicle.LicensePlate"/>
      <InputDivider/>
      <Label
        label="displaykey.Web.ClaimSnapshotGeneral310DV.Auto.Driver"/>
      <Input
        id="Driver"
        label="displaykey.Web.ClaimSnapshotGeneral310DV.Auto.Driver_1"
        value="Snapshot.Driver.DisplayName"/>
      <Input
        id="VehicleUseReason"
        label="displaykey.Web.ClaimSnapshotGeneral310DV.Auto.VehicleUseReason"
        value="Snapshot.VehicleUseReason"/>
      <Input
        id="DriverRelation2"
        label="displaykey.Web.ClaimSnapshotGeneral310DV.Auto.DriverRelation2"
        value="Snapshot.DriverRelation"/>
      <InputDivider/>
      <Label
        label="displaykey.Web.ClaimSnapshotGeneral310DV.Auto.Claim"/>
      <Input
        id="CreateTime"
        label="displaykey.Web.ClaimSnapshotGeneral310DV.Auto.CreateTime"
        value="util.Snapshot.renderValue(Snapshot.CreateTime)"/>
      <Input
        id="CloseDate"
        label="displaykey.Web.ClaimSnapshotGeneral310DV.Auto.CloseDate"
        value="util.Snapshot.renderValue(Snapshot.CloseDate)"/>
      <Input
        id="ClosedOutcome"
        label="displaykey.Web.ClaimSnapshotGeneral310DV.Auto.ClosedOutcome"
        value="Snapshot.ClosedOutcome"/>
      <BooleanRadioInput
        falseLabel="displaykey.Web.ClaimSnapshotGeneral310DV.Auto.FirstNoticeSuit"
        id="IncidentReport"
        label="displaykey.Web.ClaimSnapshotGeneral310DV.Auto.IncidentReport_1"
        trueLabel="displaykey.Web.ClaimSnapshotGeneral310DV.Auto.FirstNoticeSuit_2"
        value="Snapshot.IncidentReport"/>
      <BooleanRadioInput
        falseLabel="displaykey.Web.ClaimSnapshotGeneral310DV.Auto.FirstNoticeSuit"
        id="CoverageInQuestion"
        label="displaykey.Web.ClaimSnapshotGeneral310DV.Auto.CoverageInQuestion_1"
        trueLabel="displaykey.Web.ClaimSnapshotGeneral310DV.Auto.FirstNoticeSuit_2"
        value="Snapshot.CoverageInQuestion"/>
      <Input
        id="DeductibleStatus"
        label="displaykey.Web.ClaimSnapshotGeneral310DV.Auto.DeductibleStatus"
        value="Snapshot.DeductibleStatus"/>
      <Input
        id="LitigationStatus"
        label="displaykey.Web.ClaimSnapshotGeneral310DV.Auto.LitigationStatus"
        value="Snapshot.LitigationStatus"/>
      <Input
        id="SubrogationStatus"
        label="displaykey.Web.ClaimSnapshotGeneral310DV.Auto.SubrogationStatus"
        value="Snapshot.SubrogationStatus"/>
      <Input
        id="SalvageStatus"
        label="displaykey.Web.ClaimSnapshotGeneral310DV.Auto.SalvageStatus"
        value="Snapshot.SalvageStatus"/>
      <Input
        id="OtherRecovStatus"
        label="displaykey.Web.ClaimSnapshotGeneral310DV.Auto.OtherRecovStatus"
        value="Snapshot.OtherRecovStatus"/>
      <Input
        id="SIUStatus"
        label="displaykey.Web.ClaimSnapshotGeneral310DV.Auto.SIUStatus"
        value="Snapshot.SIUStatus"/>
      <Input
        id="ReinsuranceStatus"
        label="displaykey.Web.ClaimSnapshotGeneral310DV.Auto.ReinsuranceStatus"
        value="Snapshot.ReinsuranceStatus"/>
      <InputDivider/>
      <Label
        label="displaykey.Web.ClaimSnapshotGeneral310DV.Auto.Flag"/>
      <Input
        id="Flagged"
        label="displaykey.Web.ClaimSnapshotGeneral310DV.Auto.Flagged"
        value="Snapshot.Flagged"/>
      <Input
        id="FlaggedDate"
        label="displaykey.Web.ClaimSnapshotGeneral310DV.Auto.FlaggedDate"
        value="util.Snapshot.renderValue(Snapshot.FlaggedDate)"/>
      <Input
        id="FlaggedReason"
        label="displaykey.Web.ClaimSnapshotGeneral310DV.Auto.FlaggedReason"
        value="Snapshot.FlaggedReason"/>
      <InputDivider/>
      <Label
        label="displaykey.Web.ClaimSnapshotGeneral310DV.Auto.Officials"/>
      <ListViewInput
        labelAbove="true">
        <Toolbar/>
        <ListViewPanel
          id="OfficialsLV">
          <RowIterator
            editable="false"
            elementName="Official"
            value="Snapshot.Officials">
            <Row>
              <Cell
                id="Type"
                label="displaykey.Web.ClaimSnapshotGeneral310DV.Auto.Type"
                value="Official.OfficialType"/>
              <Cell
                id="Name"
                label="displaykey.Web.ClaimSnapshotGeneral310DV.Auto.Name"
                value="Official.Name"/>
              <Cell
                id="ReportNumber"
                label="displaykey.Web.ClaimSnapshotGeneral310DV.Auto.ReportNumber"
                value="Official.ReportNumber"/>
            </Row>
          </RowIterator>
        </ListViewPanel>
      </ListViewInput>
    </InputColumn>
  </DetailViewPanel>
</PCF>
