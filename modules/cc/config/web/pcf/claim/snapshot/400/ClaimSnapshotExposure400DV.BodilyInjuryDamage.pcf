<?xml version="1.0"?>
<PCF
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="../../../../../../../pcf.xsd">
  <DetailViewPanel
    id="ClaimSnapshotExposure400DV"
    mode="BodilyInjuryDamage|MedPay">
    <Require
      name="Claim"
      type="Claim"/>
    <Require
      name="Exposure"
      type="snapshot.v400.Exposure"/>
    <InputColumn>
      <Label
        label="displaykey.Web.ClaimSnapshotExposure400DV.Bodilyinjurydamage.General"/>
      <Input
        id="LossParty"
        label="displaykey.Web.ClaimSnapshotExposure400DV.Bodilyinjurydamage.LossParty"
        value="Exposure.LossParty"/>
      <Input
        id="PrimaryCoverage"
        label="displaykey.Web.ClaimSnapshotExposure400DV.Bodilyinjurydamage.PrimaryCoverage"
        value="Exposure.PrimaryCoverage"/>
      <Input
        id="CoverageSubType"
        label="displaykey.Web.ClaimSnapshotExposure400DV.Bodilyinjurydamage.CoverageSubType"
        value="Exposure.CoverageSubType"/>
      <Input
        id="Coverage"
        label="displaykey.Web.ClaimSnapshotExposure400DV.Bodilyinjurydamage.Coverage"
        value="util.Snapshot.renderValue(Exposure.Coverage)"/>
      <Input
        id="CreateTime"
        label="displaykey.Web.ClaimSnapshotExposure400DV.Bodilyinjurydamage.CreateTime"
        value="util.Snapshot.renderValue(Exposure.CreateTime)"/>
      <Input
        id="StatLine"
        label="displaykey.Web.ClaimSnapshotExposure400DV.Bodilyinjurydamage.StatLine"
        value="Exposure.StatLine"/>
      <InputDivider/>
      <Label
        label="displaykey.Web.ClaimSnapshotExposure400DV.Bodilyinjurydamage.Claimant"/>
      <Input
        id="Claimant"
        label="displaykey.Web.ClaimSnapshotExposure400DV.Bodilyinjurydamage.Claimant"
        value="util.Snapshot.renderValue(Exposure.Claimant)"/>
      <Input
        id="ClaimantType"
        label="displaykey.Web.ClaimSnapshotExposure400DV.Bodilyinjurydamage.ClaimantType"
        value="Exposure.ClaimantType"/>
      <BooleanRadioInput
        falseLabel="displaykey.Web.ClaimSnapshotExposure400DV.Bodilyinjurydamage.ContactProhibited"
        id="ContactProhibited"
        label="displaykey.Web.ClaimSnapshotExposure400DV.Bodilyinjurydamage.ContactProhibited_1"
        trueLabel="displaykey.Web.ClaimSnapshotExposure400DV.Bodilyinjurydamage.ContactProhibited_2"
        value="util.Snapshot.getClaimant(Exposure.Claim, Exposure)[&quot;ContactProhibited&quot;]"/>
      <Input
        id="PrimaryPhone"
        label="displaykey.Web.ClaimSnapshotExposure400DV.Bodilyinjurydamage.PrimaryPhone"
        value="util.Snapshot.getPrimaryPhone(Exposure.Claimant)"/>
      <Input
        id="PrimaryAddress"
        label="displaykey.Web.ClaimSnapshotExposure400DV.Bodilyinjurydamage.PrimaryAddress"
        value="Exposure.Claimant.PrimaryAddress.DisplayName"/>
      <Input
        id="AltContact"
        label="displaykey.Web.ClaimSnapshotExposure400DV.Bodilyinjurydamage.AltContact"
        value="util.Snapshot.renderValue(Exposure.AltContact)"/>
      <InputDivider/>
      <Label
        label="displaykey.Web.ClaimSnapshotExposure400DV.Bodilyinjurydamage.Injury"/>
      <Input
        id="Severity"
        label="displaykey.Web.ClaimSnapshotExposure400DV.Bodilyinjurydamage.Severity"
        value="Exposure.Severity"/>
      <Input
        id="LeadingInjury"
        label="displaykey.Web.ClaimSnapshotExposure400DV.Bodilyinjurydamage.LeadingInjury"
        value="Exposure.LeadingInjury"/>
      <Input
        id="DetailedInjury"
        label="displaykey.Web.ClaimSnapshotExposure400DV.Bodilyinjurydamage.DetailedInjury"
        value="Exposure.DetailedInjury"/>
      <Input
        id="PrimaryBodyPart"
        label="displaykey.Web.ClaimSnapshotExposure400DV.Bodilyinjurydamage.PrimaryBodyPart"
        value="Exposure.PrimaryBodyPart"/>
      <Input
        id="DetailedBodyPart"
        label="displaykey.Web.ClaimSnapshotExposure400DV.Bodilyinjurydamage.DetailedBodyPart"
        value="Exposure.DetailedBodyPart"/>
      <TextAreaInput
        id="Description"
        label="displaykey.Web.ClaimSnapshotExposure400DV.Bodilyinjurydamage.Description"
        numCols="30"
        numRows="3"
        value="Exposure.Incident.Description"/>
      <Input
        id="PrimaryDoctor"
        label="displaykey.Web.ClaimSnapshotExposure400DV.Bodilyinjurydamage.PrimaryDoctor"
        value="util.Snapshot.renderValue(Exposure.PrimaryDoctor)"/>
      <Input
        id="MedicalTreatment"
        label="displaykey.Web.ClaimSnapshotExposure400DV.Bodilyinjurydamage.MedicalTreatment"
        value="Exposure.MedicalTreatment"/>
      <BooleanRadioInput
        falseLabel="displaykey.Web.ClaimSnapshotExposure400DV.Bodilyinjurydamage.ContactProhibited"
        id="LostWages"
        label="displaykey.Web.ClaimSnapshotExposure400DV.Bodilyinjurydamage.LostWages_1"
        trueLabel="displaykey.Web.ClaimSnapshotExposure400DV.Bodilyinjurydamage.ContactProhibited_2"
        value="Exposure.LostWages"/>
      <Input
        id="Impairment"
        label="displaykey.Web.ClaimSnapshotExposure400DV.Bodilyinjurydamage.Impairment"
        value="Exposure.Impairment"/>
      <Input
        id="LossEstimate"
        label="displaykey.Web.ClaimSnapshotExposure400DV.Bodilyinjurydamage.LossEstimate"
        value="Exposure.LossEstimate"/>
      <InputDivider/>
    </InputColumn>
    <InputColumn>
      <Label
        label="displaykey.Web.ClaimSnapshotExposure400DV.Bodilyinjurydamage.Coding"/>
      <Input
        id="JurisdictionState"
        label="displaykey.Web.ClaimSnapshotExposure400DV.Bodilyinjurydamage.JurisdictionState"
        value="Exposure.JurisdictionState"/>
      <InputDivider/>
      <Label
        label="displaykey.Web.ClaimSnapshotExposure400DV.Bodilyinjurydamage.Other"/>
      <BooleanRadioInput
        falseLabel="displaykey.Web.ClaimSnapshotExposure400DV.Bodilyinjurydamage.ContactProhibited"
        id="OtherCoverage"
        label="displaykey.Web.ClaimSnapshotExposure400DV.Bodilyinjurydamage.OtherCoverage_1"
        trueLabel="displaykey.Web.ClaimSnapshotExposure400DV.Bodilyinjurydamage.ContactProhibited_2"
        value="Exposure.OtherCoverage"/>
      <ListViewInput
        label="displaykey.Web.ClaimSnapshotExposure400DV.Bodilyinjurydamage.Details">
        <Toolbar/>
        <ListViewPanel
          id="OtherCoverageDetLV">
          <RowIterator
            editable="false"
            elementName="OtherCoverageDet"
            value="Exposure.OtherCoverageDet">
            <Row>
              <Cell
                id="Insurer"
                label="displaykey.Web.ClaimSnapshotExposure400DV.Bodilyinjurydamage.Insurer"
                value="OtherCoverageDet.Insurer"/>
              <Cell
                id="PolicyNumber"
                label="displaykey.Web.ClaimSnapshotExposure400DV.Bodilyinjurydamage.PolicyNumber"
                value="OtherCoverageDet.PolicyNumber"/>
              <Cell
                id="ContactName"
                label="displaykey.Web.ClaimSnapshotExposure400DV.Bodilyinjurydamage.ContactName"
                value="OtherCoverageDet.ContactName"/>
              <Cell
                id="ContactPhone"
                label="displaykey.Web.ClaimSnapshotExposure400DV.Bodilyinjurydamage.ContactPhone"
                value="OtherCoverageDet.ContactPhone"/>
              <Cell
                id="Notes"
                label="displaykey.Web.ClaimSnapshotExposure400DV.Bodilyinjurydamage.Notes"
                value="OtherCoverageDet.Notes"/>
            </Row>
          </RowIterator>
        </ListViewPanel>
      </ListViewInput>
      <InputDivider/>
    </InputColumn>
  </DetailViewPanel>
</PCF>
