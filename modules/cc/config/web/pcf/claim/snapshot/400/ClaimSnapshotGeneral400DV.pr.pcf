<?xml version="1.0"?>
<PCF
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="../../../../../../../pcf.xsd">
  <DetailViewPanel
    id="ClaimSnapshotGeneral400DV"
    mode="pr">
    <Require
      name="Claim"
      type="Claim"/>
    <Require
      name="Snapshot"
      type="snapshot.v400.Claim"/>
    <InputColumn>
      <Label
        label="displaykey.Web.ClaimSnapshotGeneral400DV.Pr.General"/>
      <Input
        id="LOBCode"
        label="displaykey.Web.ClaimSnapshotGeneral400DV.Pr.LOBCode"
        value="Snapshot.LOBCode"/>
      <Input
        id="LossType"
        label="displaykey.Web.ClaimSnapshotGeneral400DV.Pr.LossType"
        value="Snapshot.LossType"/>
      <Input
        id="Catastrophe"
        label="displaykey.Web.ClaimSnapshotGeneral400DV.Pr.Catastrophe"
        value="util.Snapshot.renderValue(Snapshot.Catastrophe)"/>
      <Input
        id="PermissionRequired"
        label="displaykey.Web.ClaimSnapshotGeneral400DV.Pr.PermissionRequired"
        value="Snapshot.PermissionRequired"/>
      <InputDivider/>
      <Label
        label="displaykey.Web.ClaimSnapshotGeneral400DV.Pr.Description"/>
      <TextAreaInput
        id="Description"
        numCols="30"
        numRows="3"
        value="Snapshot.Description"/>
      <InputDivider/>
      <Label
        label="displaykey.Web.ClaimSnapshotGeneral400DV.Pr.Loss"/>
      <Input
        id="LossCause"
        label="displaykey.Web.ClaimSnapshotGeneral400DV.Pr.LossCause"
        value="Snapshot.LossCause"/>
      <Input
        id="Fault"
        label="displaykey.Web.ClaimSnapshotGeneral400DV.Pr.Fault"
        value="Snapshot.FaultRating"/>
      <DateInput
        dateFormat="short"
        id="LossDate"
        label="displaykey.Web.ClaimSnapshotGeneral400DV.Pr.LossDate"
        timeFormat="short"
        value="Snapshot.LossDate"/>
      <Input
        id="AtFaultPercentage"
        label="displaykey.Web.ClaimSnapshotGeneral400DV.Pr.AtFaultPercentage"
        value="Snapshot.FaultRating"
        visible="Snapshot.FaultRating[&quot;Code&quot;] == &quot;1&quot; as Object"/>
      <InputDivider/>
      <Label
        label="displaykey.Web.ClaimSnapshotGeneral400DV.Pr.Notification"/>
      <Input
        id="Reporter"
        label="displaykey.Web.ClaimSnapshotGeneral400DV.Pr.Reporter"
        value="Snapshot.Reporter.DisplayName"/>
      <Input
        id="MainContact"
        label="displaykey.Web.ClaimSnapshotGeneral400DV.Pr.MainContact"
        value="Snapshot.MainContact.DisplayName"/>
      <Input
        id="MainContactType"
        label="displaykey.Web.ClaimSnapshotGeneral400DV.Pr.MainContactType"
        value="Snapshot.MainContactType"/>
      <DateInput
        dateFormat="short"
        id="ReportedDate"
        label="displaykey.Web.ClaimSnapshotGeneral400DV.Pr.ReportedDate"
        value="Snapshot.ReportedDate"/>
      <Input
        id="FirstNoticeSuit"
        label="displaykey.Web.ClaimSnapshotGeneral400DV.Pr.FirstNoticeSuit"
        value="util.Snapshot.renderValue(Snapshot.FirstNoticeSuit)"/>
      <Input
        id="HowReported"
        label="displaykey.Web.ClaimSnapshotGeneral400DV.Pr.HowReported"
        value="Snapshot.HowReported"/>
      <BooleanRadioInput
        id="IncidentReport"
        label="displaykey.Web.ClaimSnapshotGeneral400DV.Pr.IncidentReport"
        value="Snapshot.IncidentReport"/>
      <BooleanRadioInput
        id="CoverageInQuestion"
        label="displaykey.Web.ClaimSnapshotGeneral400DV.Pr.CoverageInQuestion"
        value="Snapshot.CoverageInQuestion"/>
    </InputColumn>
    <InputColumn>
      <Label
        label="displaykey.Web.ClaimSnapshotGeneral400DV.Pr.Properties"/>
      <ListViewInput
        labelAbove="true">
        <Toolbar/>
        <ListViewPanel
          id="PropertiesLV">
          <RowIterator
            editable="false"
            elementName="prop"
            type="snapshot.v400.FixedPropertyIncident"
            value="util.Snapshot.filterBeansOfType(Snapshot.Incidents, &quot;FixedPropertyIncident&quot;)">
            <Row>
              <Cell
                action="ClaimSnapshotFixedPropertyIncidentPopup.push(Claim, prop)"
                id="Address1"
                label="displaykey.Web.ClaimSnapshotGeneral400DV.Pr.PropertyAddress1"
                value="prop.Property.Address.AddressLine1"/>
              <Cell
                id="City"
                label="displaykey.Web.ClaimSnapshotGeneral400DV.Pr.PropertyCity"
                value="prop.Property.Address.City"/>
              <Cell
                id="State"
                label="displaykey.Web.ClaimSnapshotGeneral400DV.Pr.PropertyState"
                value="prop.Property.Address.State"/>
            </Row>
          </RowIterator>
        </ListViewPanel>
      </ListViewInput>
      <InputDivider/>
      <Label
        label="displaykey.Web.ClaimSnapshotGeneral400DV.Pr.Loss_1"/>
      <Input
        id="LossLocation"
        label="displaykey.Web.ClaimSnapshotGeneral400DV.Pr.LossLocation"
        value="Snapshot.LossLocation.DisplayName"/>
      <Input
        id="Address1"
        label="displaykey.Web.ClaimSnapshotGeneral400DV.Pr.Address1"
        value="Snapshot.LossLocation.AddressLine1"/>
      <Input
        id="Address2"
        label="displaykey.Web.ClaimSnapshotGeneral400DV.Pr.Address2"
        value="Snapshot.LossLocation.AddressLine2"/>
      <Input
        id="City"
        label="displaykey.Web.ClaimSnapshotGeneral400DV.Pr.City"
        value="Snapshot.LossLocation.City"/>
      <Input
        id="State"
        label="displaykey.Web.ClaimSnapshotGeneral400DV.Pr.State"
        value="Snapshot.LossLocation.State"/>
      <Input
        id="PostalCode"
        label="displaykey.Web.ClaimSnapshotGeneral400DV.Pr.ZipCode"
        value="Snapshot.LossLocation.PostalCode"/>
      <Input
        id="County"
        label="displaykey.Web.ClaimSnapshotGeneral400DV.Pr.County"
        value="Snapshot.LossLocation.County"/>
      <Input
        id="Country"
        label="displaykey.Web.ClaimSnapshotGeneral400DV.Pr.Country"
        value="Snapshot.LossLocation.Country"/>
      <Input
        id="LossLocationCode"
        label="displaykey.Web.ClaimSnapshotGeneral400DV.Pr.LossLocationCode"
        value="Snapshot.LossLocationCode"/>
      <Input
        id="JurisdictionState"
        label="displaykey.Web.ClaimSnapshotGeneral400DV.Pr.JurisdictionState"
        value="Snapshot.JurisdictionState"/>
      <InputDivider/>
      <Label
        label="displaykey.Web.ClaimSnapshotGeneral400DV.Pr.Officials"/>
      <ListViewInput
        labelAbove="true">
        <Toolbar/>
        <ListViewPanel
          id="OfficialsLV">
          <RowIterator
            editable="false"
            elementName="Official"
            value="Snapshot.Officials">
            <Row>
              <Cell
                id="Type"
                label="displaykey.Web.ClaimSnapshotGeneral400DV.Pr.Type"
                value="Official.OfficialType"/>
              <Cell
                id="Name"
                label="displaykey.Web.ClaimSnapshotGeneral400DV.Pr.Name"
                value="Official.Name"/>
              <Cell
                id="ReportNumber"
                label="displaykey.Web.ClaimSnapshotGeneral400DV.Pr.ReportNumber"
                value="Official.ReportNumber"/>
            </Row>
          </RowIterator>
        </ListViewPanel>
      </ListViewInput>
      <InputDivider/>
      <Label
        label="displaykey.Web.ClaimSnapshotGeneral400DV.Pr.Claim"/>
      <DateInput
        dateFormat="short"
        id="CreateTime"
        label="displaykey.Web.ClaimSnapshotGeneral400DV.Pr.CreateTime"
        timeFormat="short"
        value="Snapshot.CreateTime"/>
      <Input
        id="ValidationLevel"
        label="displaykey.Web.ClaimSnapshotGeneral400DV.Pr.ValidationLevel"
        value="Snapshot.ValidationLevel"/>
    </InputColumn>
    <InputFooterSection>
      <ListViewInput
        boldLabel="true"
        label="displaykey.Web.ClaimSnapshotGeneral400DV.Pr.MetroReports"
        labelAbove="true">
        <Toolbar/>
        <ListViewPanel
          id="MetroReportsLV">
          <RowIterator
            editable="false"
            elementName="MetroReport"
            value="Snapshot.MetroReports">
            <Row>
              <Cell
                id="MetroReportType"
                label="displaykey.Web.ClaimSnapshotGeneral400DV.Pr.MetroReportType"
                value="MetroReport.MetroReportType"/>
              <Cell
                id="MetroReportStatus"
                label="displaykey.Web.ClaimSnapshotGeneral400DV.Pr.MetroReportStatus"
                value="MetroReport.Status"/>
              <Cell
                id="MetroReportOrderDate"
                label="displaykey.Web.ClaimSnapshotGeneral400DV.Pr.MetroReportOrderDate"
                value="MetroReport.CreateTime"/>
            </Row>
          </RowIterator>
        </ListViewPanel>
      </ListViewInput>
    </InputFooterSection>
  </DetailViewPanel>
</PCF>
