<?xml version="1.0"?>
<PCF
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="../../../../../../pcf.xsd">
  <LocationGroup
    canVisit="gw.api.snapshot.ClaimSnapshotUtil.hasSnapshot(Claim) and perm.Claim.view(Claim) and perm.System.viewsnapshot and (perm.System.viewclaimbasics or perm.System.viewclaimparties or perm.System.viewpolicy or perm.System.viewexposures or perm.System.viewclaimnotes or perm.System.viewdocs)"
    id="ClaimSnapshotGroup"
    title="displaykey.JSP.ClaimPageLinks.PageLink.Snapshot">
    <LocationEntryPoint
      signature="ClaimSnapshotGroup(Claim : Claim)"/>
    <Variable
      name="Claim"
      type="Claim"/>
    <LocationRef
      label="displaykey.JSP.ClaimSnapshot.Navigation.LossDetails"
      location="ClaimSnapshotLossDetails(Claim)"/>
    <LocationRef
      label="displaykey.JSP.ClaimSnapshot.Navigation.PeopleInvolved"
      location="ClaimSnapshotPartiesInvolved(Claim)"
      shortcut="I"/>
    <LocationRef
      label="displaykey.JSP.ClaimSnapshot.Navigation.Policy"
      location="ClaimSnapshotPolicy(Claim)"
      shortcut="L"/>
    <LocationRef
      label="displaykey.JSP.ClaimSnapshot.Navigation.Exposures"
      location="ClaimSnapshotExposures(Claim)"
      shortcut="E"/>
    <LocationRef
      label="displaykey.JSP.ClaimSnapshot.Navigation.Notes"
      location="ClaimSnapshotNotes(Claim)"
      shortcut="S"/>
    <LocationRef
      label="displaykey.JSP.ClaimSnapshot.Navigation.Documents"
      location="ClaimSnapshotDocuments(Claim)"
      shortcut="T"/>
    <LocationRef
      label="displaykey.JSP.ClaimSnapshot.Navigation.ExtraFields"
      location="ClaimSnapshotExtraFields(Claim)"
      shortcut="F"/>
  </LocationGroup>
</PCF>
