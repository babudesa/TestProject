      name="Claim"
      type="Claim"/>
    <LocationRef
      label="displaykey.JSP.ClaimParties.Contacts.Tab"
      location="ClaimContacts(Claim)"
      shortcut="T"/>
    <LocationRef
      label="displaykey.JSP.ClaimParties.Users.Tab"
      location="ClaimUsers(Claim)"
      shortcut="S"/>
  </LocationGroup>
</PCF>
