    canVisit="perm.Claim.view(Claim) and perm.System.viewclaimparties and (Claim.State != ClaimState.TC_DRAFT)"
    id="ClaimPartiesGroup"
    title="displaykey.JSP.ClaimPageLinks.PageLink.PeopleInvolved">
