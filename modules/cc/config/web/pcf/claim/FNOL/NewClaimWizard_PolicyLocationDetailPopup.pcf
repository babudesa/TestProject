<?xml version="1.0"?>
<PCF
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="../../../../../../pcf.xsd">
  <Popup
    canEdit="true"
    id="NewClaimWizard_PolicyLocationDetailPopup"
    startInEditMode="!Policy.Verified"
    title="displaykey.Wizard.NewClaimWizard.NewClaimPolicyLocation.Edit">
    <LocationEntryPoint
      signature="NewClaimWizard_PolicyLocationDetailPopup(Policy : Policy, LocationBasedRU : LocationBasedRU)"/>
    <Variable
      name="Policy"
      type="Policy"/>
    <Variable
      name="LocationBasedRU"
      type="LocationBasedRU"/>
    <Screen
      id="NewClaimWizard_PolicyLocationDetailScreen">
      <Toolbar
        reflectOnBottom="true">
        <EditButtons
          editVisible="!Policy.Verified"/>
      </Toolbar>
      <PanelRef
        def="LocationDetailPanelSet(LocationBasedRU)"
        mode="Policy.Claim.LossType"/>
    </Screen>
  </Popup>
</PCF>
