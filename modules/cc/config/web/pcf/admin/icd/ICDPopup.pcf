<?xml version="1.0"?>
<PCF
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="../../../../../../pcf.xsd">
  <Popup
    canEdit="true"
    canVisit="true"
    id="ICDPopup"
    returnType="ICDCode"
    title="displaykey.Web.Admin.ICD.ICDPopup">
    <LocationEntryPoint
      signature="ICDPopup()"/>
    <LocationEntryPoint
      signature="ICDPopup(anICD: ICDCode)"/>
    <Variable
      initialValue="new ICDCode()"
      name="anICD"
      type="ICDCode"/>
    <Screen
      editable="true"
      id="ICDPopupScreen">
      <Toolbar>
        <EditButtons
          pickValue="anICD"/>
      </Toolbar>
      <PanelRef
        def="ICDDV(anICD,false)"
        editable="true"
        id="ICDDVPanel"/>
    </Screen>
  </Popup>
</PCF>
