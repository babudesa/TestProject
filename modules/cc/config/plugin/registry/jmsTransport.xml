<?xml version="1.0"?>
<plugin
  interface="MessageTransport"
  name="jmsTransport">
  <plugin-java
    disabled="true"
    javaclass="examples.plugins.messaging.JMSMessageTransport"
    plugindir="messaging">
    <param
      name="url"
      value="rmi://localhost:1099/"/>
    <param
      name="connectionFactoryName"
      value="JmsQueueConnectionFactory"/>
    <param
      name="sendQueue"
      value="outbound"/>
  </plugin-java>
</plugin>
