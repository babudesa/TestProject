<?xml version="1.0"?>
<plugin
  interface="MessageReply"
  name="jmsReply">
  <plugin-java
    disabled="true"
    javaclass="examples.plugins.messaging.JMSMessageReply"
    plugindir="messaging">
    <param
      name="url"
      value="rmi://localhost:1099/"/>
    <param
      name="connectionFactoryName"
      value="JmsQueueConnectionFactory"/>
    <param
      name="replyQueue"
      value="outbound"/>
  </plugin-java>
</plugin>
