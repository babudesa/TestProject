<?xml version="1.0"?>
<plugin
  name="IAddressBookAdapter">
  <!-- AddressBook

       By default ClaimCenter uses the AddressBookDemoAdapter.  To use
       ContactCenter instead do the following:
       1.  Disable the plugin-java tag for AddressBookDemoAdapter (below)
       2.  Enable the plugin-gosu tag IAddressBookAdapter (below) -->
  <plugin-java
    javaclass="com.guidewire.cc.plugin.addressbook.internal.AddressBookDemoAdapter"/>
</plugin>
