<?xml version="1.0"?>
<extension
  xmlns="http://guidewire.com/datamodel"
  entityName="Exposure">
  <column
    default="foo"
    desc="varchar(30) extension; default value of 'foo'"
    name="VarcharExt"
    nullok="true"
    type="varchar">
    <columnParam
      name="size"
      value="30"/>
  </column>
  <column
    default="12"
    desc="integer extension; default value of '12'"
    name="IntegerExt"
    nullok="true"
    type="integer"/>
</extension>
