<?xml version="1.0"?>
<typelistextension
  xmlns="http://guidewire.com/typelists"
  desc="Types of Currencies."
  name="Currency">
  <typecode
    code="aud"
    desc="Australian Dollar"
    name="AUD"
    priority="5"/>
  <typecode
    code="cad"
    desc="Canadian Dollar"
    name="CAD"
    priority="4"/>
  <typecode
    code="eur"
    desc="Euro"
    name="EUR"
    priority="2"/>
  <typecode
    code="gbp"
    desc="United Kingdom Pound"
    name="GBP"
    priority="3"/>
  <typecode
    code="rub"
    desc="Russian Ruble"
    name="RUB"
    priority="6"/>
  <!-- Priority determined by frequency of usage: http://www.xe.com/popularity.php -->
  <typecode
    code="usd"
    desc="US Dollar"
    name="USD"
    priority="1"/>
  <typefilter
    desc="Valid currencies for amounts on claim and exposure metric limits."
    name="metriclimit">
    <include
      code="usd"/>
    <include
      code="cad"/>
  </typefilter>
</typelistextension>
