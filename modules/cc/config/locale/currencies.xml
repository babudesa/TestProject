<?xml version="1.0" encoding="UTF-8"?>

<Currencies xmlns="http://guidewire.com/currencies">

  <!-- Set default/base in config.xml with "DefaultApplicationCurrency" param. -->

  <CurrencyType code="usd" desc="US Dollar" storageScale="2">
    <CurrencyFormat positivePattern="$#"
                    negativePattern="($#)"
                    zeroValue="-" />
  </CurrencyType>

  <CurrencyType code="eur" desc="Euro" storageScale="2">
    <CurrencyFormat positivePattern="&#8364;# EUR"
                    negativePattern="(&#8364;#) EUR"
                    zeroValue="-" />
  </CurrencyType>

  <CurrencyType code="gbp" desc="UK Pound" storageScale="2">
    <CurrencyFormat positivePattern="&#163;# GBP"
                    negativePattern="(&#163;#) GBP"
                    zeroValue="-" />
  </CurrencyType>

  <CurrencyType code="cad" desc="Canadian Dollar" storageScale="2">
    <CurrencyFormat positivePattern="$# CAD"
                    negativePattern="($#) CAD"
                    zeroValue="-" />
  </CurrencyType>

  <CurrencyType code="aud" desc="Australian Dollar" storageScale="2">
    <CurrencyFormat positivePattern="$# AUD"
                    negativePattern="($#) AUD"
                    zeroValue="-" />
  </CurrencyType>
  <CurrencyType code="rub" desc="Russian Ruble" storageScale="2">
    <CurrencyFormat positivePattern="&#1088;# RUB"
                    negativePattern="(&#1088;#) RUB"
                    zeroValue="-" />
  </CurrencyType>

</Currencies>
