<?xml version="1.0"?>
<Localization
  xmlns="http://guidewire.com/localization">
  <!-- United States English; the default if nothing else is modified -->
  <GWLocale
    code="en_US"
    name="English (US)"
    typecode="en_US">
    <DateFormat
      long="E, MMM d, yyyy"
      medium="MMM d, yyyy"
      short="MM/dd/yyyy"/>
    <TimeFormat
      long="hh:mm aa"
      medium="hh:mm aa"
      short="hh:mm aa"/>
    <NumberFormat
      decimalSymbol="."
      thousandsSymbol=","/>
    <CurrencyFormat
      negativePattern="($#)"
      positivePattern="$#"
      zeroValue="-"/>
  </GWLocale>
</Localization>
