<?xml version="1.0"?>
<entity
  xmlns="http://guidewire.com/datamodel"
  desc="Text, Logical and Numeric Answers supplied in a specific Question Set. Question Sets are groupings of questions utilized in the risk qualification process or to develop supplemental underwriting information. Specific to Claim Center"
  entity="Answer"
  exportable="true"
  extendable="true"
  javaClass="com.guidewire.cc.domain.question.Answer"
  platform="false"
  table="answer"
  type="retireable">
  <implementsEntity
    name="AnswerDelegate"/>
  <implementsEntity
    name="Extractable"/>
  <foreignkey
    columnName="AnswerSetID"
    desc="Fk to the Answer Set this answer corresponds to"
    fkentity="AnswerSet"
    name="AnswerSet"
    nullok="true"/>
</entity>
