<?xml version="1.0"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
  
  <!-- permission can be any ClaimAccessType code securitylevel can be any ClaimSecurityType
       code or the string "any" -->
  <xsd:element name="AccessLevel">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded"/>
      <xsd:attribute name="level" type="AccessLevelType" use="required"/>
      <xsd:attribute name="permission" type="xsd:NMTOKEN" use="required"/>
    </xsd:complexType>
  </xsd:element>
  
  <xsd:element name="AccessMapping">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded"/>
      <xsd:attribute name="claimAccessType" type="xsd:NMTOKEN" use="required"/>
      <xsd:attribute name="systemPermission" type="xsd:NMTOKEN" use="required"/>
    </xsd:complexType>
  </xsd:element>
  
  <xsd:element name="AccessProfile">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="ActivityAccessLevels" minOccurs="1" maxOccurs="1"/>
        <xsd:element ref="ClaimAccessLevels" minOccurs="1" maxOccurs="1"/>
        <xsd:element ref="ClaimOwnPermission" minOccurs="0" maxOccurs="1"/>
        <xsd:element ref="ExposureAccessLevels" minOccurs="1" maxOccurs="1"/>
        <xsd:element ref="SubObjectOwnPermission" minOccurs="0" maxOccurs="1"/>
      </xsd:choice>
      <xsd:attribute name="securitylevel" type="xsd:NMTOKEN" use="required"/>
    </xsd:complexType>
  </xsd:element>
  
  <xsd:element name="ActivityAccessLevels">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="AccessLevel" minOccurs="0" maxOccurs="unbounded"/>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
  
  <xsd:element name="ClaimAccessLevels">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="AccessLevel" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="ClaimUserAccessLevel" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="DraftClaimAccessLevel" minOccurs="0" maxOccurs="1"/>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
  
  <xsd:element name="ClaimOwnPermission">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded"/>
      <xsd:attribute name="permission" type="xsd:NMTOKEN" use="required"/>
    </xsd:complexType>
  </xsd:element>
  
  <!-- role can be any ClaimUserRole permission can be any ClaimAccessType code securitylevel
       can be any ClaimSecurityType code or the string "any" -->
  <xsd:element name="ClaimUserAccessLevel">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded"/>
      <xsd:attribute name="level" type="AccessLevelType" use="required"/>
      <xsd:attribute name="permission" type="xsd:NMTOKEN" use="required"/>
      <xsd:attribute name="role" type="xsd:NMTOKEN" use="required"/>
    </xsd:complexType>
  </xsd:element>
  
  <xsd:element name="ContactCreatePermission">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded"/>
      <xsd:attribute name="permission" type="SystemPerm" use="required"/>
    </xsd:complexType>
  </xsd:element>
  
  <xsd:element name="ContactDeletePermission">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded"/>
      <xsd:attribute name="permission" type="SystemPerm" use="required"/>
    </xsd:complexType>
  </xsd:element>
  
  <xsd:element name="ContactEditPermission">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded"/>
      <xsd:attribute name="permission" type="SystemPerm" use="required"/>
    </xsd:complexType>
  </xsd:element>
  
  <xsd:simpleType name="ContactEntity">
    <xsd:restriction base="xsd:NMTOKEN">
      <xsd:enumeration value="Contact"/>
    </xsd:restriction>
  </xsd:simpleType>
  
  <!-- Contact-related permissions -->
  <xsd:element name="ContactPermissions">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="ContactSubtypeAccessProfile" minOccurs="0" maxOccurs="unbounded"/>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
  
  <!-- Encapsulates the permission mappings for a Contact action based on Contact subtype -->
  <xsd:element name="ContactSubtypeAccessProfile">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="ContactCreatePermission" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="ContactDeletePermission" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="ContactEditPermission" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="ContactViewPermission" minOccurs="0" maxOccurs="unbounded"/>
      </xsd:choice>
      <xsd:attribute name="entity" type="ContactEntity" use="required"/>
    </xsd:complexType>
  </xsd:element>
  
  <xsd:element name="ContactViewPermission">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded"/>
      <xsd:attribute name="permission" type="SystemPerm" use="required"/>
    </xsd:complexType>
  </xsd:element>
  
  <xsd:element name="DocumentAccessProfile">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="DocumentCreatePermission" minOccurs="0" maxOccurs="1"/>
        <xsd:element ref="DocumentDeletePermission" minOccurs="0" maxOccurs="1"/>
        <xsd:element ref="DocumentEditPermission" minOccurs="0" maxOccurs="1"/>
        <xsd:element ref="DocumentViewPermission" minOccurs="0" maxOccurs="1"/>
      </xsd:choice>
      <xsd:attribute name="securitylevel" type="DocumentSecurityLevel" use="required"/>
    </xsd:complexType>
  </xsd:element>
  
  <xsd:element name="DocumentCreatePermission">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded"/>
      <xsd:attribute name="permission" type="SystemPerm" use="required"/>
    </xsd:complexType>
  </xsd:element>
  
  <xsd:element name="DocumentDeletePermission">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded"/>
      <xsd:attribute name="permission" type="SystemPerm" use="required"/>
    </xsd:complexType>
  </xsd:element>
  
  <xsd:element name="DocumentEditPermission">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded"/>
      <xsd:attribute name="permission" type="SystemPerm" use="required"/>
    </xsd:complexType>
  </xsd:element>
  
  <!-- Encapsulates the permission mappings for a given DocumentSecurityType value -->
  <xsd:element name="DocumentPermissions">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="DocumentAccessProfile" minOccurs="0" maxOccurs="unbounded"/>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
  
  <xsd:simpleType name="DocumentSecurityLevel">
    <xsd:restriction base="xsd:NMTOKEN">
      <!-- Document that is sensitive in nature -->
      <xsd:enumeration value="sensitive"/>
      <!-- Document that does not require access restriction -->
      <xsd:enumeration value="unrestricted"/>
    </xsd:restriction>
  </xsd:simpleType>
  
  <xsd:element name="DocumentViewPermission">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded"/>
      <xsd:attribute name="permission" type="SystemPerm" use="required"/>
    </xsd:complexType>
  </xsd:element>
  
  <xsd:element name="DraftClaimAccessLevel">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded"/>
      <xsd:attribute name="level" type="AccessLevelType" use="required"/>
    </xsd:complexType>
  </xsd:element>
  
  <xsd:simpleType name="Entity">
    <xsd:restriction base="xsd:NMTOKEN">
      <xsd:enumeration value="Activity"/>
      <xsd:enumeration value="ActivityPattern"/>
      <xsd:enumeration value="Attribute"/>
      <xsd:enumeration value="AttributeUser"/>
      <xsd:enumeration value="AuthorityLimit"/>
      <xsd:enumeration value="AuthorityLimitProfile"/>
      <xsd:enumeration value="BulkInvoice"/>
      <xsd:enumeration value="BusinessWeek"/>
      <xsd:enumeration value="Catastrophe"/>
      <xsd:enumeration value="Check"/>
      <xsd:enumeration value="Claim"/>
      <xsd:enumeration value="ClaimAssociation"/>
      <xsd:enumeration value="ClaimInfo"/>
      <xsd:enumeration value="ClaimMetricLimit"/>
      <xsd:enumeration value="Contact"/>
      <xsd:enumeration value="Credential"/>
      <xsd:enumeration value="Document"/>
      <xsd:enumeration value="Evaluation"/>
      <xsd:enumeration value="ExchangeRate"/>
      <xsd:enumeration value="Exposure"/>
      <xsd:enumeration value="Group"/>
      <xsd:enumeration value="GroupRegion"/>
      <xsd:enumeration value="GroupUser"/>
      <xsd:enumeration value="Holiday"/>
      <xsd:enumeration value="Incident"/>
      <xsd:enumeration value="Matter"/>
      <xsd:enumeration value="Negotiation"/>
      <xsd:enumeration value="Note"/>
      <xsd:enumeration value="Payment"/>
      <xsd:enumeration value="Policy"/>
      <xsd:enumeration value="Recovery"/>
      <xsd:enumeration value="RecoveryReserve"/>
      <xsd:enumeration value="RecoveryReserveSet"/>
      <xsd:enumeration value="RecoverySet"/>
      <xsd:enumeration value="Region"/>
      <xsd:enumeration value="Reserve"/>
      <xsd:enumeration value="ReserveSet"/>
      <xsd:enumeration value="Review"/>
      <xsd:enumeration value="Role"/>
      <xsd:enumeration value="RolePrivilege"/>
      <xsd:enumeration value="ScriptParameter"/>
      <xsd:enumeration value="SecurityZone"/>
      <xsd:enumeration value="Transaction"/>
      <xsd:enumeration value="TransactionSet"/>
      <xsd:enumeration value="User"/>
      <xsd:enumeration value="UserRole"/>
    </xsd:restriction>
  </xsd:simpleType>
  
  <xsd:element name="ExposureAccessLevels">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="AccessLevel" minOccurs="0" maxOccurs="unbounded"/>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
  
  <xsd:element name="ExposurePermission">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded"/>
      <xsd:attribute name="permission" type="xsd:NMTOKEN" use="required"/>
      <xsd:attribute name="securitylevel" type="xsd:NMTOKEN"/>
    </xsd:complexType>
  </xsd:element>
  
  <xsd:element name="ExposurePermissions">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="ExposurePermission" minOccurs="0" maxOccurs="unbounded"/>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
  
  <!-- Encapsulates the permission mappings for a given NoteSecurityType value -->
  <xsd:element name="NoteAccessProfile">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="NoteCreatePermission" minOccurs="0" maxOccurs="1"/>
        <xsd:element ref="NoteDeletePermission" minOccurs="0" maxOccurs="1"/>
        <xsd:element ref="NoteEditBodyPermission" minOccurs="0" maxOccurs="1"/>
        <xsd:element ref="NoteEditPermission" minOccurs="0" maxOccurs="1"/>
        <xsd:element ref="NoteViewPermission" minOccurs="0" maxOccurs="1"/>
      </xsd:choice>
      <xsd:attribute name="securitylevel" type="xsd:NMTOKEN" use="required"/>
    </xsd:complexType>
  </xsd:element>
  
  <xsd:element name="NoteCreatePermission">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded"/>
      <xsd:attribute name="permission" type="SystemPerm" use="required"/>
    </xsd:complexType>
  </xsd:element>
  
  <xsd:element name="NoteDeletePermission">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded"/>
      <xsd:attribute name="permission" type="SystemPerm" use="required"/>
    </xsd:complexType>
  </xsd:element>
  
  <xsd:element name="NoteEditBodyPermission">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded"/>
      <xsd:attribute name="permission" type="SystemPerm" use="required"/>
    </xsd:complexType>
  </xsd:element>
  
  <xsd:element name="NoteEditPermission">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded"/>
      <xsd:attribute name="permission" type="SystemPerm" use="required"/>
    </xsd:complexType>
  </xsd:element>
  
  <!-- Note-related permissions, allowing a mapping between actions such as view, edit, etc.
       and a required system permission based on the Note's SecurityType property -->
  <xsd:element name="NotePermissions">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="NoteAccessProfile" minOccurs="0" maxOccurs="unbounded"/>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
  
  <xsd:simpleType name="NoteSecurityLevel">
    <xsd:restriction base="xsd:NMTOKEN">
      <!-- Medical note, viewable by internal users with medical note view/edit permissions -->
      <xsd:enumeration value="medical"/>
      <!-- Note viewable by internal users only -->
      <xsd:enumeration value="private"/>
      <!-- Note viewable by any user in the system -->
      <xsd:enumeration value="public"/>
      <!-- Confidential note, viewable by select internal users only -->
      <xsd:enumeration value="sensitive"/>
    </xsd:restriction>
  </xsd:simpleType>
  
  <xsd:element name="NoteViewPermission">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded"/>
      <xsd:attribute name="permission" type="SystemPerm" use="required"/>
    </xsd:complexType>
  </xsd:element>
  
  <xsd:element name="SecurityConfig">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="AccessMapping" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="AccessProfile" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="ContactPermissions" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="ContactPermissions" minOccurs="0" maxOccurs="1"/>
        <xsd:element ref="DocumentPermissions" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="DocumentPermissions" minOccurs="0" maxOccurs="1"/>
        <xsd:element ref="ExposurePermissions" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="NotePermissions" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="NotePermissions" minOccurs="0" maxOccurs="1"/>
        <xsd:element ref="StaticHandler" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="WrapHandler" minOccurs="0" maxOccurs="unbounded"/>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
  
  <!-- This will create a static handler for the given permission -->
  <xsd:element name="StaticHandler">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="SystemPermType" minOccurs="0" maxOccurs="unbounded"/>
      </xsd:choice>
      <xsd:attribute name="desc" type="xsd:string"/>
      <!-- The entity for the permission key and permision check -->
      <xsd:attribute name="entity" type="Entity" use="required"/>
      <xsd:attribute name="noPermissionDisplayKey" type="xsd:string"/>
      <!-- The permission key -->
      <xsd:attribute name="permKey" type="xsd:NMTOKEN" use="required"/>
    </xsd:complexType>
  </xsd:element>
  
  <xsd:element name="SubObjectOwnPermission">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded"/>
      <xsd:attribute name="permission" type="xsd:NMTOKEN" use="required"/>
    </xsd:complexType>
  </xsd:element>
  
  <xsd:simpleType name="SystemPerm">
    <xsd:restriction base="xsd:NMTOKEN">
      <!-- Permission to edit claim storage information -->
      <xsd:enumeration value="StorageUpdate"/>
      <!-- Permission to create a new contact in the address book -->
      <xsd:enumeration value="abcreate"/>
      <!-- Permission to add a preferred vendor to the address book -->
      <xsd:enumeration value="abcreatepref"/>
      <!-- Permission to delete an existing contact in the address book -->
      <xsd:enumeration value="abdelete"/>
      <!-- Permission to delete an existing preferred vendor address book entry -->
      <xsd:enumeration value="abdeletepref"/>
      <!-- Permission to edit an existing contact in the address book -->
      <xsd:enumeration value="abedit"/>
      <!-- Permission to modify an existing preferred vendor address book entry -->
      <xsd:enumeration value="abeditpref"/>
      <!-- Permission to view the details of contact entries in the address book -->
      <xsd:enumeration value="abview"/>
      <!-- Permission to search contact entries in the address book -->
      <xsd:enumeration value="abviewsearch"/>
      <!-- Permission to approve any approval activity even if the activity is assigned to someone
           else; the approver is still subject to authority limit restrictions -->
      <xsd:enumeration value="actapproveany"/>
      <!-- Permission to create new activities -->
      <xsd:enumeration value="actcreate"/>
      <!-- Permission to create a new activity associated with a closed claim -->
      <xsd:enumeration value="actcreateclsd"/>
      <!-- Permission to modify (edit/skip/close) activities owned by other users -->
      <xsd:enumeration value="acteditunowned"/>
      <!-- Permission to set whether an activity is mandatory -->
      <xsd:enumeration value="actmakemand"/>
      <!-- Permission to own an activity and to see the Desktop Activities page -->
      <xsd:enumeration value="actown"/>
      <!-- Permission to create new activity patterns -->
      <xsd:enumeration value="actpatcreate"/>
      <!-- Permission to delete activity patterns -->
      <xsd:enumeration value="actpatdelete"/>
      <!-- Permission to edit activity patterns -->
      <xsd:enumeration value="actpatedit"/>
      <!-- Permission to view the list of activity patterns or activity pattern details -->
      <xsd:enumeration value="actpatview"/>
      <!-- Permission to get the next activity off of a queue -->
      <xsd:enumeration value="actqueuenext"/>
      <!-- Permission to pick an activity from a queue -->
      <xsd:enumeration value="actqueuepick"/>
      <!-- Permission to reassign your own activities -->
      <xsd:enumeration value="actraown"/>
      <!-- Permission to reassign activities owned by other users -->
      <xsd:enumeration value="actraunown"/>
      <!-- Permission to review and approve manually-approved assignables -->
      <xsd:enumeration value="actreviewassign"/>
      <!-- Permission to view activities -->
      <xsd:enumeration value="actview"/>
      <!-- Permission to view all activity queues, even those in other security zones -->
      <xsd:enumeration value="actviewallqueues"/>
      <!-- Permission create, edit, or delete authority limit profiles -->
      <xsd:enumeration value="alpmanage"/>
      <!-- Permission to view authority limit profiles -->
      <xsd:enumeration value="alpview"/>
      <!-- Permission to archive objects -->
      <xsd:enumeration value="archive"/>
      <!-- Permission to create, edit, or delete claim associations -->
      <xsd:enumeration value="assocmanage"/>
      <!-- Permission to view claim associations on a claim -->
      <xsd:enumeration value="assocview"/>
      <!-- Permission to create, edit, or delete user attributes -->
      <xsd:enumeration value="attrmanage"/>
      <!-- Permission to view the list of user attributes or attribute details -->
      <xsd:enumeration value="attrview"/>
      <!-- Permission to create bulk invoices -->
      <xsd:enumeration value="bulkinvcreate"/>
      <!-- Permission to delete bulk invoices -->
      <xsd:enumeration value="bulkinvdelete"/>
      <!-- Permission to edit bulk invoices -->
      <xsd:enumeration value="bulkinvedit"/>
      <!-- Permission to view bulk invoices -->
      <xsd:enumeration value="bulkinvview"/>
      <!-- Permission to create, edit, or delete business week -->
      <xsd:enumeration value="buswkmanage"/>
      <!-- Permission to view the list of business week -->
      <xsd:enumeration value="buswkview"/>
      <!-- Permission to create, edit, and activate/deactivate catastrophes -->
      <xsd:enumeration value="catmanage"/>
      <!-- Permission to view the list of catastrophes and details of individual catastrophes -->
      <xsd:enumeration value="catview"/>
      <!-- Permission to transfer a check -->
      <xsd:enumeration value="checktransfer"/>
      <!-- Permission to close a claim -->
      <xsd:enumeration value="claimclose"/>
      <!-- Permission to create a new claim -->
      <xsd:enumeration value="claimcreate"/>
      <!-- Permission to edit a claim -->
      <xsd:enumeration value="claimedit"/>
      <!-- Permission to edit a closed claim -->
      <xsd:enumeration value="claimeditclsd"/>
      <!-- Permission to perform integration admin functions on a claim -->
      <xsd:enumeration value="claimintegadmin"/>
      <!-- Permission to own a claim and see the Desktop Claims page -->
      <xsd:enumeration value="claimown"/>
      <!-- Permission to print a claim -->
      <xsd:enumeration value="claimprint"/>
      <!-- Permission to create, edit, or delete parties from a claim -->
      <xsd:enumeration value="claimprtsmanage"/>
      <!-- Permission to manage parties involved on a closed claim -->
      <xsd:enumeration value="claimprtsmanclsd"/>
      <!-- Permission to reassign your own claims -->
      <xsd:enumeration value="claimraown"/>
      <!-- Permission to reassign claims owned by other users -->
      <xsd:enumeration value="claimraunown"/>
      <!-- Permission to remove the flag from flagged claims -->
      <xsd:enumeration value="claimremflag"/>
      <!-- Permission to reopen a claim -->
      <xsd:enumeration value="claimreopen"/>
      <!-- Permission to select a new policy for an existing claim -->
      <xsd:enumeration value="claimselectplcy"/>
      <!-- Permission to select a new policy for a closed claim -->
      <xsd:enumeration value="claimslplcyclsd"/>
      <!-- Permission to manage claim users -->
      <xsd:enumeration value="claimusermanage"/>
      <!-- Permission to manage claim users on a closed claim -->
      <xsd:enumeration value="claimusermanclsd"/>
      <!-- Permission to run validation rules on a claim -->
      <xsd:enumeration value="claimvalidate"/>
      <!-- General permission to view a claim -->
      <xsd:enumeration value="claimview"/>
      <!-- Permission to view checks and payments (and derived information) on a claim -->
      <xsd:enumeration value="claimviewpay"/>
      <!-- Permission to view recoveries (and derived information) on a claim -->
      <xsd:enumeration value="claimviewrec"/>
      <!-- Permission to view recovery reserves (and derived information) on a claim -->
      <xsd:enumeration value="claimviewrecres"/>
      <!-- Permission to view reserves (and derived information) on a claim -->
      <xsd:enumeration value="claimviewres"/>
      <!-- Permission to void a cleared check -->
      <xsd:enumeration value="clearedpayvoid"/>
      <!-- Permission to create, edit, and delete Coverage Verification values -->
      <xsd:enumeration value="covverifymanage"/>
      <!-- Permission to view the list of Coverage Verification values -->
      <xsd:enumeration value="covverifyview"/>
      <!-- Permission to create a new local contact -->
      <xsd:enumeration value="ctccreate"/>
      <!-- Permission to edit an existing local contact -->
      <xsd:enumeration value="ctcedit"/>
      <!-- Permission to view and search local contact entries -->
      <xsd:enumeration value="ctcview"/>
      <!-- Permission to access debug tools, even when they are disabled by a configuration parameter -->
      <xsd:enumeration value="debugtools"/>
      <!-- Permission to delete a medical note -->
      <xsd:enumeration value="delmednote"/>
      <!-- Permission to delete a private note -->
      <xsd:enumeration value="delprivnote"/>
      <!-- Permission to delete a sensitive document -->
      <xsd:enumeration value="delsensdoc"/>
      <!-- Permission to delete a sensitive note -->
      <xsd:enumeration value="delsensnote"/>
      <!-- Permission to add documents -->
      <xsd:enumeration value="doccreate"/>
      <!-- Permission to add documents to a closed claim -->
      <xsd:enumeration value="doccreateclsd"/>
      <!-- Permission to remove documents -->
      <xsd:enumeration value="docdelete"/>
      <!-- Permission to remove documents from a closed claim -->
      <xsd:enumeration value="docdeleteclsd"/>
      <!-- Permission to edit documents -->
      <xsd:enumeration value="docedit"/>
      <!-- Permission to edit or delete all documents, regardless of the permissions set on the
           individual documents -->
      <xsd:enumeration value="docmodifyall"/>
      <!-- Permission to view documents -->
      <xsd:enumeration value="docview"/>
      <!-- Permission to view all documents, regardless of the permissions set on the individual
           documents -->
      <xsd:enumeration value="docviewall"/>
      <!-- Permission to view the Dashboard claim activity screen -->
      <xsd:enumeration value="edbclaimact"/>
      <!-- Permission to view the Dashboard claim counts screen -->
      <xsd:enumeration value="edbclaimcounts"/>
      <!-- Permission to view the Dashboard current financials screen -->
      <xsd:enumeration value="edbcurrfin"/>
      <!-- Permission to view the Dashboard period financials screen -->
      <xsd:enumeration value="edbpdfin"/>
      <!-- Permission to edit sensitive Medical Case Management details -->
      <xsd:enumeration value="editSensMCMdetails"/>
      <!-- Permission to edit sensitive SIU details -->
      <xsd:enumeration value="editSensSIUdetails"/>
      <!-- Permission to edit Deductible -->
      <xsd:enumeration value="editdeductible"/>
      <!-- Permission to edit a medical note -->
      <xsd:enumeration value="editmednote"/>
      <!-- Permission to edit a private note -->
      <xsd:enumeration value="editprivnote"/>
      <!-- Permission to edit administration reference data -->
      <xsd:enumeration value="editrefdata"/>
      <!-- Permission to edit a sensitive document -->
      <xsd:enumeration value="editsensdoc"/>
      <!-- Permission to edit a sensitive note -->
      <xsd:enumeration value="editsensnote"/>
      <!-- Permission to edit Subrogation-related information -->
      <xsd:enumeration value="editsubrodetails"/>
      <!-- Permission to create an evaluation on a claim -->
      <xsd:enumeration value="evalcreate"/>
      <!-- Permission to edit an evaluation on a claim -->
      <xsd:enumeration value="evaledit"/>
      <!-- Permission to edit an evaluation on a closed claim -->
      <xsd:enumeration value="evaleditclsd"/>
      <!-- Permission to view an evaluation on a claim -->
      <xsd:enumeration value="evalview"/>
      <!-- Permission to view the event messages page -->
      <xsd:enumeration value="eventmessageview"/>
      <!-- Permission to override exchange rate with a custom exchange rate. -->
      <xsd:enumeration value="exchratemanual"/>
      <!-- Permission to close an exposure -->
      <xsd:enumeration value="expclose"/>
      <!-- Permission to create a new exposure -->
      <xsd:enumeration value="expcreate"/>
      <!-- Permission to edit an exposure on a claim -->
      <xsd:enumeration value="expedit"/>
      <!-- Permission to edit a closed exposure -->
      <xsd:enumeration value="expeditclsd"/>
      <!-- Permission to own an exposure and to see the Desktop Exposures page -->
      <xsd:enumeration value="expown"/>
      <!-- Permission to reassign your own exposures -->
      <xsd:enumeration value="expraown"/>
      <!-- Permission to reassign exposures owned by other users -->
      <xsd:enumeration value="expraunown"/>
