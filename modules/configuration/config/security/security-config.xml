      <ClaimUserAccessLevel role="corporateaccount" level="user" permission="view"/>
      <ClaimUserAccessLevel role="corporateaccount" level="user" permission="edit"/>
    </ClaimAccessLevels>
    <ActivityAccessLevels>
      <!--<AccessLevel level="user" permission="view"/>-->
      <!--<AccessLevel level="user" permission="edit"/>-->
   </ActivityAccessLevels>
    <ExposureAccessLevels>
      <AccessLevel level="user" permission="view"/>
      <AccessLevel level="user" permission="edit"/>
    </ExposureAccessLevels>
  </AccessProfile>
 <!--*** end Coverage Secure claim ***-->
 
 <!-- ** 4/14/14 Unsecure Claim **-->
 <!-- 4/18/14 Any LOB that makes Unsecure claims will use this ACL Profile -->
 <!-- 8/6/14 Note: We cannot make AccessLevel 'anyone' otherwise TPA Secure could see in -->
  <AccessProfile securitylevel="unsecuredclaim">
    <ClaimAccessLevels>
      <AccessLevel level="securityzone" permission="view"/>
      <AccessLevel level="securityzone" permission="edit"/>
      <!--<DraftClaimAccessLevel level="securityzone"/>-->
      <ClaimUserAccessLevel role="sensitiveclaim" level="securityzone" permission="view"/>
      <ClaimUserAccessLevel role="sensitiveclaim" level="securityzone" permission="edit"/>
      <!--<ClaimUserAccessLevel role="tempclaimeditor" level="user" permission="view"/>-->
      <!--<ClaimUserAccessLevel role="tempclaimeditor" level="user" permission="edit"/>-->
      <ClaimUserAccessLevel role="transferclaim" level="securityzone" permission="view"/>
      <ClaimUserAccessLevel role="transferclaim" level="securityzone" permission="edit"/>
    </ClaimAccessLevels>
    <ActivityAccessLevels>
      <!--<AccessLevel level="user" permission="view"/>-->
      <!--<AccessLevel level="user" permission="edit"/>-->
    </ActivityAccessLevels>
    <ExposureAccessLevels>
      <AccessLevel level="user" permission="view"/>
      <AccessLevel level="user" permission="edit"/>
    </ExposureAccessLevels>
  </AccessProfile>
  <!-- ** end Unsecure Claim **-->
  
 

