  <!--** 4/4/13 Secure claim for FidCrime K&R or any LOB that wants a Secure claim use this profile**-->
  <!-- 4/18/14 Any LOB that wants a Secure claim will use this ACL Profile -->
  <AccessProfile securitylevel="secureclaim">
    <ClaimAccessLevels>
      <AccessLevel level="securityzone" permission="view"/>
      <AccessLevel level="securityzone" permission="edit"/>
      <!--<DraftClaimAccessLevel level="securityzone"/>-->
      <ClaimUserAccessLevel role="secureclaim" level="user" permission="view"/>
      <ClaimUserAccessLevel role="secureclaim" level="user" permission="edit"/>
      <ClaimUserAccessLevel role="tempclaimeditor" level="user" permission="view"/>
      <ClaimUserAccessLevel role="tempclaimeditor" level="user" permission="edit"/>
    </ClaimAccessLevels>
    <ActivityAccessLevels>
      <!--<AccessLevel level="user" permission="view"/>-->
      <!--<AccessLevel level="user" permission="edit"/>-->
    </ActivityAccessLevels>
    <ExposureAccessLevels>
      <AccessLevel level="user" permission="view"/>
      <AccessLevel level="user" permission="edit"/>
    </ExposureAccessLevels>
  </AccessProfile>
 <!--***** end Secure claim for FidCrime K&R or any LOB that wants a Secure claim *****-->
 
 <!--** Secure External (Assigned Secure)- Secure claim type that is assigned outside of its Security Zone **-->
 <!-- The userRole of Secure Adjuster is only necessary for the UI, so the UI does not error, it
 	  is not needed in the security-config to grant access.  They would have access by being
 	  assigned to the claim 1/15/15 ER -->
  <AccessProfile securitylevel="secureexternal">
    <ClaimAccessLevels>
      <AccessLevel level="user" permission="view"/>
      <AccessLevel level="user" permission="edit"/>
      <!--<DraftClaimAccessLevel level="securityzone"/>-->
      <ClaimUserAccessLevel role="transferclaim" level="securityzone" permission="view"/>
      <ClaimUserAccessLevel role="transferclaim" level="securityzone" permission="edit"/>
      <ClaimUserAccessLevel role="sensitiveclaim" level="user" permission="view"/>
      <ClaimUserAccessLevel role="sensitiveclaim" level="user" permission="edit"/>
      <ClaimUserAccessLevel role="secureclaim" level="user" permission="view"/>
      <ClaimUserAccessLevel role="secureclaim" level="user" permission="edit"/>
      <ClaimUserAccessLevel role="tempclaimeditor" level="user" permission="view"/>
      <ClaimUserAccessLevel role="tempclaimeditor" level="user" permission="edit"/>
    </ClaimAccessLevels>
    <ActivityAccessLevels>
      <!--<AccessLevel level="securityzone" permission="view"/>-->
      <!--<AccessLevel level="securityzone" permission="edit"/>-->
    </ActivityAccessLevels>
    <ExposureAccessLevels>
      <AccessLevel level="user" permission="view"/>
      <AccessLevel level="user" permission="edit"/>
    </ExposureAccessLevels>
  </AccessProfile>
 <!--**** end Secure External(Assigned Secure) claim ****-->

 <!-- 4/18/14 Any LOB that wants a Coverage Secure claim will use this ACL Profile -->
  <AccessProfile securitylevel="coveragesecure">
    <ClaimAccessLevels>
      <ClaimUserAccessLevel role="secureadjuster" level="user" permission="view"/>
      <ClaimUserAccessLevel role="secureadjuster" level="user" permission="edit"/>
