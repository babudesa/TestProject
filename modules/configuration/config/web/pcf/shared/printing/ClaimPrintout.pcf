            locationRef="ClaimHistory(Claim)"/>
        </PrintOption>
      </PrintSection>
    </PrintGroup>
    <!-- ==============================================================================================================
      Current claim file page without details
      ============================================================================================================== -->
    <PrintLocation
      id="CurrentClaimFilePagePrint"
      label="displaykey.Java.PrintClaimOptionsForm.Label.ThisPageWithoutDetails"/>
    <!-- ==============================================================================================================
      Current claim file page with details
      ============================================================================================================== -->
    <PrintLocation
      id="CurrentClaimFilePagePrintWithDetails"
      label="displaykey.Java.PrintClaimOptionsForm.Label.ThisPageWithDetails">
      <PrintLocationDetail
        baseLocation="ClaimWorkplan"
        locationRef="ActivityDetailPrint(Activity)"
        symbolName="Activity"
        symbolType="Activity"/>
      <PrintLocationDetail
        baseLocation="ClaimAssociations"
        locationRef="ClaimAssociationDetail(Claim, ClaimAssociation)"
        symbolName="ClaimAssociation"
        symbolType="ClaimAssociation"/>
      <PrintLocationDetail
        baseLocation="ClaimExposures"
        locationRef="ExposureDetail(Exposure)"
        symbolName="Exposure"
        symbolType="Exposure"/>
      <PrintLocationDetail
        baseLocation="ClaimContacts"
        listViewRef="PeopleInvolvedDetailedLV"
        locationRef="ClaimContactPrintDetails(Claim, selectedClaimContact)"
        symbolName="selectedClaimContact"
        symbolType="ClaimContact"/>
      <PrintLocationDetail
        baseLocation="ClaimUsers"
        locationRef="ClaimUserPrintDetails(Claim, ClaimUserModel)"
        symbolName="ClaimUserModel"
        symbolType="ClaimUserModel"/>
      <PrintLocationDetail
        baseLocation="ClaimPolicyVehicles"
        listViewRef="VehiclesLV"
        locationRef="ClaimPolicyVehicleDetail(Claim, VehicleRU)"
        printable="helper.isAutoClaim()"
        symbolName="VehicleRU"
        symbolType="VehicleRU"/>
      <PrintLocationDetail
        baseLocation="ClaimPolicyLocations"
        listViewRef="LocationsLV"
        locationRef="ClaimPolicyLocationDetail(Claim, LocationBasedRU)"
        printable="canShowClaimPolicyLocations"
        symbolName="LocationBasedRU"
        symbolType="LocationBasedRU"/>
      <PrintLocationDetail
        baseLocation="ClaimPolicyEndorsements"
        listViewRef="EndorsementsLV"
        locationRef="ClaimPolicyEndorsementDetail(Claim, Endorsement)"
        symbolName="Endorsement"
        symbolType="Endorsement"/>
      <!-- 6/12/2008 - zthomas - Defect 273, added printable parameter to check for StatCodes policy tab before calling ClaimPolicyStatCodes. -->
      <PrintLocationDetail
        baseLocation="ClaimPolicyStatCodes"
        listViewRef="StatCodesLV"
        locationRef="ClaimPolicyStatCodeDetail(Claim, StatCode)"
        printable="gw.api.policy.PolicyTabUtil.hasTab(Claim, &quot;Statcodes&quot;)"
        symbolName="StatCode"
        symbolType="StatCode"/>
      <!-- 6/12/2008 - zthomas - Defect 273, added printable parameter to check for AggregateLimits policy tab before calling ClaimPolicyAggregateLimits. -->
      <PrintLocationDetail
        baseLocation="ClaimPolicyAggregateLimits"
        listViewRef="AggregateLimitsLV"
        locationRef="ClaimPolicyAggregateLimitDetail(Claim, AggregateLimit)"
        printable="gw.api.policy.PolicyTabUtil.hasTab(Claim, &quot;AggregateLimits&quot;)"
        symbolName="AggregateLimit"
        symbolType="AggregateLimit"/>
      <PrintLocationDetail
        baseLocation="ClaimFinancialsTransactions"
        locationRef="TransactionDetailPrint(Claim, TransactionView)"
        symbolName="TransactionView"
