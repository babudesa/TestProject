    <Code><![CDATA[function createPrintSettings() : print.PrintSettings {
        var newPrintSettings = new print.PrintSettings();
        var claimNumberLabel = displaykey.Web.PrintOut.ClaimNumber(Claim.ClaimNumber);
        newPrintSettings.setHeaderLabel(claimNumberLabel);
        newPrintSettings.InternalSettings.excludeWidget("NoteSearchDV");
        newPrintSettings.InternalSettings.excludeWidget("ClaimDocumentSearchDV");
        newPrintSettings.InternalSettings.excludeWidget("ClaimNotesTitle");
        newPrintSettings.InternalSettings.excludeWidget("StatCodeFilterDV");
        return newPrintSettings;
      }

      function canPrintClaimSnapshot(c : Claim) : boolean {
        return gw.api.snapshot.ClaimSnapshotUtil.hasSnapshot(c)
          and perm.Claim.view(c)
          and perm.System.viewsnapshot
          and (perm.System.viewclaimbasics
            or perm.System.viewclaimparties
            or perm.System.viewpolicy
            or perm.System.viewexposures
            or perm.System.viewclaimnotes
            or perm.System.viewdocs);
      }

      function isWorkersCompClaim(c: Claim) : boolean {
        return false;
        //return (c.LossType == LossType.TC_WC);
      }

      function getMedicalDetails(c : Claim) : Exposure {
        return findExposureForCoverageSubtype(c, CoverageSubtype.TC_WM_WCID);
      }

      function getIndemityTimeLoss(c: Claim) : Exposure {
        return findExposureForCoverageSubtype(c, CoverageSubtype.TC_WI_LW);
      }

      function getEmployerLiability(c : Claim) : Exposure {
        return findExposureForCoverageSubtype(c, CoverageSubtype.TC_WL_EL);
      }

      function findExposureForCoverageSubtype(c : Claim, cst : CoverageSubtype) : Exposure {
        for (nextExposure in Claim.Exposures) {
          if (nextExposure.CoverageSubType == cst) {
            return nextExposure;
          }
        }
        return null;
      }]]></Code>
