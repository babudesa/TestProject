<?xml version="1.0"?>
<PCF
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="../../../../../pcf.xsd">
  <ExitPoint
    id="DocumentExitPoint"
    popup="true"
    target="ecfMainWindow"
    url="{exitUrl}">
    <LocationEntryPoint
      signature="DocumentExitPoint(url : String)"/>
    <ExitPointParameter
      locationParam="url"
      type="String"
      urlParam="exitUrl"/>
  </ExitPoint>
</PCF>