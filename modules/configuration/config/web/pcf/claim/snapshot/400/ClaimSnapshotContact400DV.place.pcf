<?xml version="1.0"?>
<PCF
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="../../../../../../../pcf.xsd">
  <DetailViewPanel
    id="ClaimSnapshotContact400DV"
    mode="Place">
    <Require
      name="ClaimContactParam"
      type="Object"/>
    <InputHeaderSection>
      <Label
        label="displaykey.Web.ClaimSnapshotContact400DV.Place.Claim"/>
      <ListViewInput
        labelAbove="true">
        <Toolbar/>
        <ListViewPanel
          id="ClaimRolesLV">
          <RowIterator
            editable="false"
            elementName="Role"
            value="asClaimContact().Roles">
            <Row>
              <Cell
                id="Role"
                label="displaykey.Web.ClaimSnapshotContact400DV.Place.Role"
                value="Role.Role"/>
              <Cell
                id="Owner"
                label="displaykey.Web.ClaimSnapshotContact400DV.Place.Owner"
                value="util.Snapshot.getRoleOwner(Role)"/>
              <Cell
                id="Active"
                label="displaykey.Web.ClaimSnapshotContact400DV.Place.Active"
                value="util.Snapshot.renderValue(Role.Active)"/>
              <Cell
                id="Comments"
                label="displaykey.Web.ClaimSnapshotContact400DV.Place.Comments"
                value="Role.Comments"/>
            </Row>
          </RowIterator>
        </ListViewPanel>
      </ListViewInput>
      <InputDivider/>
    </InputHeaderSection>
    <InputColumn>
      <Label
        label="displaykey.Web.ClaimSnapshotContact400DV.Place.Name"/>
      <Input
        id="contact"
        label="displaykey.Web.ClaimSnapshotContact400DV.Place.Name"
        value="asClaimContact().Contact.DisplayName"/>
      <InputDivider/>
      <Label
        label="displaykey.Web.ClaimSnapshotContact400DV.Place.ContactInfo"/>
      <Input
        id="PrimaryContact"
        label="displaykey.Web.ClaimSnapshotContact400DV.Place.PrimaryContact"
        value="util.Snapshot.renderValue(asClaimContact().Contact.PrimaryContact)"/>
      <Input
        id="PrimaryPhone"
        label="displaykey.Web.ClaimSnapshotContact400DV.Place.PrimaryPhone"
        value="asClaimContact().Contact.PrimaryPhone"/>
      <Input
        id="WorkPhone"
        label="displaykey.Web.ClaimSnapshotContact400DV.Place.WorkPhone"
        value="asClaimContact().Contact.WorkPhone"/>
      <Input
        id="HomePhone"
        label="displaykey.Web.ClaimSnapshotContact400DV.Place.HomePhone"
        value="asClaimContact().Contact.HomePhone"/>
      <Input
        id="FaxPhone"
        label="displaykey.Web.ClaimSnapshotContact400DV.Place.FaxPhone"
        value="asClaimContact().Contact.FaxPhone"/>
      <InputDivider/>
      <Label
        label="displaykey.Web.ClaimSnapshotContact400DV.Place.Email"/>
      <Input
        id="EmailAddress1"
        label="displaykey.Web.ClaimSnapshotContact400DV.Place.EmailAddress1"
        value="asClaimContact().Contact.EmailAddress1"/>
      <Input
        id="EmailAddress2"
        label="displaykey.Web.ClaimSnapshotContact400DV.Place.EmailAddress2"
        value="asClaimContact().Contact.EmailAddress2"/>
      <InputDivider/>
    </InputColumn>
    <InputColumn>
      <Label
        label="displaykey.Web.ClaimSnapshotContact400DV.Place.Primary"/>
      <Input
        id="PrimaryAddress"
        label="displaykey.Web.ClaimSnapshotContact400DV.Place.PrimaryAddress"
        value="asClaimContact().Contact.PrimaryAddress.DisplayName"/>
      <Input
        id="County"
        label="displaykey.Web.ClaimSnapshotContact400DV.Place.County"
        value="asClaimContact().Contact.PrimaryAddress.County"/>
      <Input
        id="Country"
        label="displaykey.Web.ClaimSnapshotContact400DV.Place.Country"
        value="asClaimContact().Contact.PrimaryAddress.Country"/>
      <Input
        id="AddressType"
        label="displaykey.Web.ClaimSnapshotContact400DV.Place.AddressType"
        value="asClaimContact().Contact.PrimaryAddress.AddressType"/>
      <Input
        id="Description"
        label="displaykey.Web.ClaimSnapshotContact400DV.Place.Description"
        value="asClaimContact().Contact.PrimaryAddress.Description"/>
      <Input
        id="ValidUntil"
        label="displaykey.Web.ClaimSnapshotContact400DV.Place.ValidUntil"
        value="asClaimContact().Contact.PrimaryAddress.ValidUntil"/>
      <InputDivider/>
      <Label
        label="displaykey.Web.ClaimSnapshotContact400DV.Place.Notes"/>
      <TextAreaInput
        id="Notes"
        numCols="30"
        numRows="3"
        value="asClaimContact().Contact.Notes"/>
    </InputColumn>
    <Code><![CDATA[
function asClaimContact() : snapshot.v400.ClaimContact {
        return ClaimContactParam as snapshot.v400.ClaimContact;
      }
    ]]></Code>
  </DetailViewPanel>
</PCF>
