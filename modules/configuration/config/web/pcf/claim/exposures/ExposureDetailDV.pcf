            label="displaykey.Web.Medicare.ICDCodes"/>
          <ListViewInput
            def="MedicalDiagnosisLV(pageProc, injury, true)"
            labelAbove="true"
            validationExpression="util.WCHelper.validateICDCodes(injury) == true ? displaykey.Validation.Exposure.ICD19 : null">
            <Toolbar>
              <ToolbarButton
                action="ICD9CodePopup.push(pageProc, &quot;DiagnosticWC&quot;, injury)"
                available="CurrentLocation.InEditMode and pageProc.DiagnosticCodes.Count &lt; 19"
                hideIfReadOnly="true"
                id="addCode"
                label="&quot;Diagnostic Code Lookup&quot;"
                visible="injury.InjuryDiagnoses.last().ICDCode != null or injury.InjuryDiagnoses.Count == 0 ? true : false"/>
              <IteratorButtons/>
              <ToolbarDivider/>
              <CheckedValuesToolbarButton
                checkedRowAction="aMedicalDiagnosis.InjuryIncident.setPrimaryDiagnosisToFalse(); aMedicalDiagnosis.IsPrimary = true"
                hideIfReadOnly="true"
                id="Make_Primary"
                iterator="MedicalDiagnosisLV"
                label="displaykey.NVV.Claim.MedCaseMgrDetails.MedicalDiagnosis.MakePrimary"/>
            </Toolbar>
          </ListViewInput>
        </InputColumn>
      </DetailViewPanel>
      <DetailViewPanel
        editable="false">
        <InputColumn>
          <InputDivider/>
          <Label
            label="displaykey.MedicalTreatments.MedTreatmentsPrescribed"/>
          <ListViewInput
            def="MedTreatPrescribedLV(Exposure)"
            labelAbove="true">
            <Toolbar>
              <IteratorButtons
                addVisible="false"
                removeVisible="false"/>
            </Toolbar>
          </ListViewInput>
        </InputColumn>
      </DetailViewPanel>
      <!-- <PanelRef
        def="ExposureCertificateDV(Exposure.Claim, Exposure)"
        id="ExposureCertificatePanelRef"/> -->
    </Card>
    <Card
      id="Exposure_ISOCard"
      title="displaykey.NVV.Exposure.ISO"
      visible="false">
      <PanelRef
        def="ISODetailsDV(Exposure)"/>
    </Card>
    <Card
      id="Exposure_CMSCard"
      title="displaykey.NVV.Exposure.Medicare"
      visible="(Exposure.Claimant.Subtype==&quot;Person&quot;) and Exposure.MedicareExposureExt and !util.WCHelper.isWCorELLossType(Exposure.Claim)">
      <PanelRef
        def="CMSDetailsDV(Exposure)"/>
    </Card>
    <Code><![CDATA[function initContactISOMedicare(selectedContact : Contact) : ContactISOMedicareExt {
  
  if(selectedContact == null){
    return new ContactISOMedicareExt()
  }

  if(selectedContact.ContactISOMedicareExt == null && typeof selectedContact == Person){
    selectedContact.ContactISOMedicareExt = new ContactISOMedicareExt()
  }
    
  return selectedContact.ContactISOMedicareExt
}]]></Code>
