        visible="false">
        <InputColumn>
          <Label
            label="displaykey.MedCaseMgr.LVs.MedicalTreatmentsLV"/>
        </InputColumn>
      </DetailViewPanel>
      <PanelRef
        def="MedicalTreatmentsLV(Exposure.Claim)"
        editable="true"
        visible="false">
        <Toolbar>
          <IteratorButtons/>
