      name="Claim"
      type="Claim"/>
    <RowIterator
      editable="true"
      elementName="AIRisk"
      hasCheckBoxes="true"
      hideCheckBoxesIfReadOnly="true"
      toAdd="Vehicle.addToAdditionalInterestRisksExt(AIRisk)"
