    afterCancel="MatterDetailPage.go(Claim, Matter)"
    canEdit="perm.Matter.edit(Matter)"
    canVisit="perm.Matter.view(Matter)"
    id="MatterDetailPage"
    parent="ClaimMatters(Claim)"
    showUpLink="true"
    title="displaykey.Web.ClaimMatters.Matter( Matter)">
