      signature="MatterDetailPage(Claim : Claim, Matter : Matter)"/>
    <Variable
      name="Claim"
      type="Claim"/>
    <Variable
      name="Matter"
      type="Matter"/>
    <ScreenRef
      def="MatterDetailScreen(Claim, Matter)"/>
  </Page>
</PCF>