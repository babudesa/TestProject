      name="Claim"
      type="Claim"/>
    <Variable
      name="Exposures"
      type="Exposure[]"/>
    <ScreenRef
      def="ReserveReinsurancesExtDetailScreen(Claim, Exposures)"/>
  </Page>
</PCF>