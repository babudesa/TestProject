<?xml version="1.0"?>
<PCF
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="../../../../../../pcf.xsd">
  <PanelSet
    desc=""
    id="NewExpCvgDetailsPanelSet"
    mode="default">
    <Require
      name="Claim"
      type="Claim"/>
    <Require
      name="Coverages"
      type="Coverage[]"/>
    <ListDetailPanel
      selectionName="selectedCoverage"
      selectionType="Coverage">
      <PanelRef
        def="PropertyCoverageSelectionLV(Coverages)"/>
      <CardViewPanel>
        <Card
          id="NewExpCvGDetailsCard"
          title="displaykey.Web.Contact.AddressDetail.Title">
          <PanelRef
            def="ExposureSelectionDV(Claim, selectedCoverage)"/>
        </Card>
      </CardViewPanel>
    </ListDetailPanel>
  </PanelSet>
</PCF>
