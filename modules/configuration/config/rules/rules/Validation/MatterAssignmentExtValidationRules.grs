package rules.Validation

@gw.rules.RuleName("MatterAssignmentExtValidationRules")
class MatterAssignmentExtValidationRules extends gw.rules.RuleSetBase {
  static function invoke(bean : entity.MatterAssignmentExt) : gw.rules.ExecutionSession {
    return invoke( new gw.rules.ExecutionSession(), bean )
  }

  static function invoke(exeSession : gw.rules.ExecutionSession, bean : entity.MatterAssignmentExt) : gw.rules.ExecutionSession {
    return _invoke( exeSession, bean, "MatterAssignmentExtValidationRules", MatterAssignmentExtValidationRules )
  }
}
