<?xml version="1.0"?>
<FieldValidators
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="../../../pl/xsd/metadata/fieldvalidation.xsd">
  <!-- Warning:
     If you modify the Field Validator definitions, please note that you may want to review any related DisplayKeys in Display.properties.

     For example, if you were to change "Phone", you would also want to review the description which is a key in Display.Properties:
      “Validator.Phone = {0} must be a 10-  digit phone number, with an optional extension” and confirm this is still accurate.
   Field Validator Definitions: -->
  <ValidatorDef
    description="Validator.ClaimNumber"
    input-mask="#########"
    name="ClaimNumber"
    value="(([A-Za-z]{1}[0-9]{8})|([0-9]{9}))"/>
  <ValidatorDef
    description="Validator.Speed"
    format="#mph"
    input-mask=""
    name="Speed"
    value="[0-9]{1,3}([Mm][Pp][Hh])?"/>
  <ValidatorDef
    description="Validator.Phone"
    input-mask=""
    name="Phone"
    value=".+"/>
  <ValidatorDef
    description="Validator.Email"
    input-mask=""
    name="Email"
    value=".+@.+"/>
  <ValidatorDef
    description="Validator.Year"
    input-mask=""
    name="Year"
    value="[1-2]{1}[0-9]{3}?"/>
  <ValidatorDef
    description="Validator.SSN"
    input-mask=""
    name="SSN"
    value=".+"/>
  <ValidatorDef
    description="Validator.PolicyNumber"
    input-mask="#######"
    name="PolicyNumber"
    value="[0-9A-Za-z]{7}"/>
  <ValidatorDef
    description="Validator.UnverifiedPolicyNumber"
    input-mask="#######"
    name="PolicyNumber"
    value="[0-9A-Za-z]{7}"/>
  <!--<ValidatorDef
    description="Validator.VIN"
    input-mask=""
    name="AutoVIN"
    value="[0-9A-HJ-NPR-Z]{17}"/> -->
  <ValidatorDef
    description="Validator.Money"
    input-mask=""
    name="Money"
    value="-?([0-9]{1,16}|[0]{0,1})\.?[0-9]{0,2}"/>
  <ValidatorDef
    description="Validator.Risk"
    input-mask=""
    name="Risk"
    value="[0-9]{1,4}\.?[0-9]{0,2}"/>
  <ValidatorDef
    description="Validator.Exmod"
    input-mask=""
    name="Exmod"
    value="[0-9]{1,1}\.?[0-9]{0,4}"/>
  <ValidatorDef
    description="Validator.FirstName"
    input-mask=""
    name="FirstName"
    value=".+"/>
  <ValidatorDef
    description="Validator.LastName"
    input-mask=""
    name="LastName"
    value=".+"/>
  <ValidatorDef
    description="Validator.CoName"
    input-mask=""
    name="CoName"
    value=".+"/>
  <ValidatorDef
    description="Validator.EmploymentClassification"
    input-mask="######"
    name="EmploymentClassification"
    value="[0-9]{6}?"/>
  <ValidatorDef
    ceiling="100"
    description="Validator.LoadFactor"
    floor="0"
    name="LoadFactor"
    value="[0-9]+"/>
  <ValidatorDef
    ceiling="7"
    description="Validator.DaysWorkedWeek"
    floor="0.1"
    input-mask=""
    name="DaysWorkedWeek"
    value="[0-7](\.[0-9])?"/>
  <ValidatorDef
    ceiling="24"
    description="Validator.HoursWorkedDay"
    floor="0.1"
    name="HoursWorkedDay"
    value="[0-9]+(\.[0-9])?"/>
  <ValidatorDef
    description="Validator.Account"
    input-mask=""
    name="Account"
    value=".+"/>
  <ValidatorDef
    description="Validator.AddressLine"
    input-mask=""
    name="AddressLine"
    value=".+"/>
  <ValidatorDef
    description="Validator.Rate"
    input-mask=""
    name="Rate"
    value="[0-9]{1,6}\.?[0-9]{0,4}"/>
  <ValidatorDef
    description="Validator.RatingLineBasisAmount"
    input-mask=""
    name="RatingLineBasisAmount"
    value="-?[0-9]{1,20}\.?[0-9]{0,2}"/>
  <ValidatorDef
    description="Validator.ProrationFactor"
    input-mask=""
    name="ProrationFactor"
    value="[0-9]{1,1}\.?[0-9]{0,4}"/>
  <ValidatorDef
    description="Validator.CreditCardNumber"
    input-mask=""
    name="CreditCardNumber"
    value="[0-9]{4}-[0-9]{4}-[0-9]{4}-[0-9]{4}|[0-9]{4}-[0-9]{6}-[0-9]{5}"/>
  <ValidatorDef
    description="Validator.DunAndBradstreetNumber"
    input-mask=""
    name="DunAndBradstreetNumber"
    value="[0-9]{2}-[0-9]{3}-[0-9]{4}"/>
  <ValidatorDef
    description="Validator.Date"
    input-mask="##/##/####"
    name="Date"
    value="[0-1][0-9]/[0-9]{2}/[0-9]{4}"/>
  <ValidatorDef
    description="Validator.PostalCode"
    input-mask="#####-####"
    name="PostalCode"
    value=".+"/>
  
<!-- Commenting out until the business is sure they want this. sprzygocki 6/29/09
  
  <ValidatorDef
    description="Validator.AccoutingMonthExt"
    input-mask=""
    name="AccountingMonth"
    value="(0[1-9]|1[012])"/> -->
  <ValidatorDef
    description="Validator.FormEditionExt"
    input-mask="##/##"
    name="FormEditionDate"
    value="((0[1-9]|1[012])[/]([0-9][0-9]))"/>
  <ValidatorDef
    description="Validator.ProducerCode"
    input-mask="######"
    name="ProducerCode"
    value="[0-9]{6}"/>
  <ValidatorDef
    description="Validator.BoatLength"
    input-mask=""
    name="BoatLength"
    value="[0-9]{0,5}"/>
  <ValidatorDef
    description="Validator.PolicyMod"
    input-mask="##"
    name="PolicyMod"
    value="[0-9]{2}"/>
  <ValidatorDef
    description="Validator.PolicyVersion"
    input-mask="####"
    name="PolicyVersion"
    value="[0-9]{4}"/>
  <ValidatorDef
    description="Validator.MMYYYYDate"
    input-mask="##/####"
    name="MMYYYYDate"
    value="[0-1][0-9]/[0-9]{4}"/>
  <ValidatorDef
    description="Validator.Phone"
    input-mask="###-###-#### x####"
    name="OtherCoverageContPhone"
    value="[0-9]{3}-[0-9]{3}-[0-9]{4}( x[0-9]{0,4})?"/>
  <ValidatorDef
    description="Validator.HICN"
    input-mask=""
    name="HICN"
    value="[0-9a-zA-Z]{6,12}"/>
  <ValidatorDef
    description="Validator.SIUSurvHeight"
    input-mask="#-##"
    name="SIUSurvHeight"
    value="[1-9][-](0[0-9]|1[01])"/>
  <ValidatorDef
    description="Validator.SIUTravelDate"
    input-mask="##/####"
    name="SIUTravelDate"
    value="((0[1-9]|1[012])[/](19|20[0-9][0-9]))"/>
  <ValidatorDef
    description="Validator.Weight"
    input-mask="###"
    name="Weight"
    value="[0-9]{0,3}"/>
  <ValidatorDef
    description="Validator.WCImpairmentExt"
    input-mask=""
    name="WCImpairmentExt"
    value="[0-9]*"/>
  <ValidatorDef
    description="Validator.VocRehabLimitWeeksEXT"
    input-mask=""
    name="VocRehabLimitWeeksExt"
    value="[1-9][0-9]{0,2}"/>
  <ValidatorDef
    description="Validator.PilotHoursExt"
    name="PilotHoursExt"
    value="(([0-9]{0,40}))"/>
</FieldValidators>
