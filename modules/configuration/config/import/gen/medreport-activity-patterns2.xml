<?xml version="1.0"?>
<import>

<!-- Medicare Reporting Activity Patterns -->
  <ActivityPattern public-id="cms_activity:001">
    <code>med_eligibility_confirm</code>
    <subject>Medicare Eligibility confirmed by CMS</subject>
    <description>Medicare Eligibility has been confirmed. The Defer Sending to CMS field has been changed to Yes. The Stop Querying Party for CMS field has been changed to Yes.</description>
    <activityclass>task</activityclass>
    <type>general</type>
    <category>general</category>
    <claimlosstype></claimlosstype>
    <importance>high</importance>
    <mandatory>False</mandatory>
    <priority>normal</priority>
    <recurring>False</recurring>
    <targetdays>10</targetdays>
    <targethours></targethours>
    <targetincludedays>elapsed</targetincludedays>
    <targetstartpoint>activitycreation</targetstartpoint>
    <escalationdays></escalationdays>
    <escalationhours></escalationhours>
    <escalationincldays></escalationincldays>
    <escalationstartpt></escalationstartpt>
    <closedclaimavlble>False</closedclaimavlble>
    <automatedonly>True</automatedonly>
    <command></command>
    <externallyowned>False</externallyowned>
  </ActivityPattern>
  <ActivityPattern public-id="cms_activity:003">
    <code>inj_party_not_bene</code>
    <subject>Injured Party Not Identified as a Medicare Beneficiary</subject>
    <description>The claimant was reported by the Adjuster as Medicare Eligible, but he/she is not found in the CMS system, and has no known HICN number. Confirm the claimant information entered.</description>
    <activityclass>task</activityclass>
    <type>general</type>
    <category>general</category>
    <claimlosstype></claimlosstype>
    <importance>high</importance>
    <mandatory>False</mandatory>
    <priority>normal</priority>
    <recurring>False</recurring>
    <targetdays>10</targetdays>
    <targethours></targethours>
    <targetincludedays>elapsed</targetincludedays>
    <targetstartpoint>activitycreation</targetstartpoint>
    <escalationdays></escalationdays>
    <escalationhours></escalationhours>
    <escalationincldays></escalationincldays>
    <escalationstartpt></escalationstartpt>
    <closedclaimavlble>False</closedclaimavlble>
    <automatedonly>True</automatedonly>
    <command></command>
    <externallyowned>False</externallyowned>
  </ActivityPattern>
</import>
