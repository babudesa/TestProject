<?xml version="1.0"?>
<typelistextension
  xmlns="http://guidewire.com/typelists"
  desc="Custom history event types, used to add customer-defined events to a claim's history"
  name="CustomHistoryType">
  <typecode
    code="a_n_f_r"
    desc="Claim exception: Fault rating not set on auto claim"
    name="Auto: No fault rating"/>
  <typecode
    code="catastrophe"
    desc="Change to catastrophe values"
    name="Guidewire catastrophe rules"/>
  <typecode
    code="create_recovery_bill"
    desc="Create recovery bill"
    name="Create recovery bill"/>
  <typecode
    code="DataChange"
    desc="Data change"
    name="Data change"/>
  <typecode
    code="e_w_n_r"
    desc="Claim exception: No reserve set for exposure"
    name="Exposure with no reserves"/>
  <typecode
    code="email"
    desc="Email sent"
    name="Email sent"/>
  <typecode
    code="Export"
    desc="Integration: New claim exported to mainframe"
    name="Exported to mainframe"/>
  <typecode
    code="newPolicy"
    desc="A flag is set if a policy mod is 00 and Loss Date is within 60 days of inception."
    name="New Policy Mod 00 Flag"/>
  <typecode
    code="review_reserve_rec"
    desc="Idle 30 Day Reserve Recommendation"
    name="Review Reserve Recommendation"/>
  <typecode
    code="UnvPolTransApprove"
    desc="Unverified Policy Transaction Approval"
    name="Unverified Policy Transaction Approval"/>
  <typecode
    code="Vetconversion"
    desc="Flag is set if a vet originally set to primary doctor role has been converted to a doctor or MCO."
    name="Vet conversion flag"/>
  <typecode
    code="checkedit"
    desc="An item on this claim was approved/rejected"
    name="Approval or Rejection"/>
</typelistextension>
