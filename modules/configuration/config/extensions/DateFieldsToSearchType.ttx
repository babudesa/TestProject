<?xml version="1.0"?>
<typelistextension
  xmlns="http://guidewire.com/typelists"
  desc="The search options for the date searches in search"
  name="DateFieldsToSearchType">
  <typecode
    code="approved"
    desc="Find by approved date"
    name="Approved date">
    <category
      code="payment"
      typelist="SearchObjectType"/>
    <category
      code="recovery"
      typelist="SearchObjectType"/>
  </typecode>
  <typecode
    code="closed"
    desc="Find by closed date"
    name="Closed date">
    <category
      code="activity"
      typelist="SearchObjectType"/>
    <category
      code="claim"
      typelist="SearchObjectType"/>
  </typecode>
  <typecode
    code="create"
    desc="Find by creation date"
    name="Creation date">
    <category
      code="activity"
      typelist="SearchObjectType"/>
    <category
      code="claim"
      typelist="SearchObjectType"/>
    <category
      code="payment"
      typelist="SearchObjectType"/>
    <category
      code="recovery"
      typelist="SearchObjectType"/>
  </typecode>
  <typecode
    code="issue"
    desc="Find by issue date"
    name="Issue date">
    <category
      code="payment"
      typelist="SearchObjectType"/>
  </typecode>
  <typecode
    code="loss"
    desc="Find by Loss Date"
    name="Loss date">
    <category
      code="claim"
      typelist="SearchObjectType"/>
    <category
      code="claimInfo"
      typelist="SearchObjectType"/>
  </typecode>
  <typecode
    code="periodend"
    desc="Find by service period end"
    name="Service period end">
    <category
      code="payment"
      typelist="SearchObjectType"/>
  </typecode>
  <typecode
    code="periodstart"
    desc="Find by service period start"
    name="Service period start">
    <category
      code="payment"
      typelist="SearchObjectType"/>
  </typecode>
  <typecode
    code="reported"
    desc="Find by reported date"
    name="Reported date">
    <category
      code="claim"
      typelist="SearchObjectType"/>
  </typecode>
  <typecode
    code="schedsend"
    desc="The date the check is supposed to go out"
    name="Scheduled send date">
    <category
      code="payment"
      typelist="SearchObjectType"/>
  </typecode>
  <typecode
    code="service"
    desc="Find by service date"
    name="Date of service">
    <category
      code="payment"
      typelist="SearchObjectType"/>
  </typecode>
  <typecode
    code="target"
    desc="Find by target date"
    name="Due date">
    <category
      code="activity"
      typelist="SearchObjectType"/>
  </typecode>
  <typefilter
    desc="Types to use for the Exposure search"
    name="ExposureSearchTypes">
    <include
      code="create"/>
    <include
      code="closed"/>
    <include
      code="featureassigned"/>
  </typefilter>
  <typecode
    code="featureassigned"
    desc="Find by assigned date"
    name="Feature Assigned date"/>
</typelistextension>