    name="EvaluationTypeExt"
    typelist="EvaluationType"/>
  <typekey
    desc="Designates if it's a loss or expense evaluation"
    name="EvaluationForExt"
    typelist="EvaluationFor"/>
  <foreignkey
    desc="Loss Location"
    fkentity="Address"
    name="LossLocationExt"/>
  <column
    desc="The loss reserve for the designated claim or exposure."
    name="CurrentLossReserveExt"
    type="money"/>
  <column
    desc="The expense reserve for the designated claim or exposure."
    name="CurrentExpenseReserveExt"
    type="money"/>
  <column
    desc="The target amount for the settlement."
    name="SettlementTargetAmtExt"
    type="money"/>
  <column
    desc="Other deductions"
    name="OtherDeductionsExt"
    nullok="true"
    type="money"/>
  <column
    desc="The claimant name at the time the evaluation is saved to ECF"
    name="ClaimantNameExt"
    type="varchar">
    <columnParam
      name="size"
      value="200"/>
  </column>
  <column
    desc="Comments section"
    name="VocRehabComments"
    type="varchar">
    <columnParam
      name="size"
      value="4000"/>
  </column>
  <array
    arrayentity="RehabilitationExt"
    desc="Vocational Rehabilitation"
    name="Rehabs"
    triggersValidation="true"
    owner="true"/>
  <foreignkey
    desc="Evaluation for Medical"
    exportable="true"
    fkentity="MedicalEvalExt"
    name="MedicalEval"
    nullok="true"/>
  <foreignkey
    desc="Indemnity Evaluation"
    exportable="true"
    fkentity="IndemnityEvalExt"
    name="IndemnityEval"
    nullok="true"/>
</extension>
