<?xml version="1.0"?>
<!-- Evaluation -->
<extension
  xmlns="http://guidewire.com/datamodel"
  entityName="Evaluation">
  <foreignkey
    fkentity="ReserveLine"
    name="ReserveExt"/>
  <column
    desc="Occupation of the Claimant"
    name="ClaimantOccupationExt"
    nullok="true"
    type="varchar">
    <columnParam
      name="size"
      value="255"/>
  </column>
  <column
    desc="Loss Description"
    name="LossDescriptionExt"
    nullok="true"
    type="varchar">
    <columnParam
      name="size"
      value="600"/>
