    name="PainSufferingDamagesHighExt"
    nullok="true"
    type="money"/>
  <column
    desc="Other Damages Low"
    name="OtherDamagesLowExt"
    nullok="true"
    type="money"/>
  <!-- OtherDamagesHighExt added Sprint 8, jlmiller for evaluation screens -->
  <column
    desc="Other Damages High"
    name="OtherDamagesHighExt"
    nullok="true"
    type="money"/>
  <column
    desc="Other Factors"
    name="OtherFactorsExt"
    nullok="true"
    type="varchar">
    <columnParam
      name="size"
      value="600"/>
  </column>
  <column
    desc="JVV Range Low Estimate"
    name="JVVRangeLowExt"
    nullok="true"
    type="money"/>
  <column
    desc="JVV Range High Estimate"
    name="JVVRangeHighExt"
    nullok="true"
    type="money"/>
  <column
    desc="Insured Range"
    name="InsuredRangeExt"
    nullok="true"
    type="money"/>
  <column
    desc="Potential feature range low value"
    name="PotFeatRangeLowExt"
    nullok="true"
    type="money"/>
