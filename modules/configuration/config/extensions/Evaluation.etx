    <columnParam
      name="size"
      value="600"/>
  </column>
  <column
    desc="Plaintiff Strengths"
    name="PlaintiffStrengthsExt"
    nullok="true"
    type="varchar">
    <columnParam
      name="size"
      value="600"/>
  </column>
