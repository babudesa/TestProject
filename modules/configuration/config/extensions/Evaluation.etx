    name="SaveToECFExt"
    type="bit"/>
  <column
    desc="Low value of insured liability percentage"
    name="LowInsuredLiabilityExt"
    type="percentage"/>
  <column
    desc="High value of insured liability percentage"
    name="HighInsuredLiabilityExt"
    type="percentage"/>
