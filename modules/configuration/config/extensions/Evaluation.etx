  <column
    desc="Defense Costs"
    name="DefenseCostExt"
    nullok="true"
    type="money"/>
  <column
    desc="Recommended Reserve"
    name="RecReserveExt"
    nullok="true"
    type="money"/>
  <column
    desc="Reserve Change"
    name="ReserveChangeExt"
    nullok="true"
    type="money"/>
  <column
    desc="Reserve Change Reasons"
    name="ReserveChangeReasonExt"
    nullok="true"
    type="longtext"/>
  <column
    desc="Approved Reserve Amount"
    name="ApprReserveAmtExt"
    nullok="true"
    type="money"/>
  <column
    desc="Reserve Change Amount Gross"
    name="ChangeAmtGrossExt"
    nullok="true"
    type="money"/>
  <column
    desc="Reserve Change Amount New"
    name="ChangeAmtNewExt"
    nullok="true"
    type="money"/>
  <column
    desc="Approved By"
    name="ApprovedByExt"
    nullok="true"
    type="varchar">
    <columnParam
      name="size"
      value="255"/>
  </column>
  <column
    desc="Reserve Basis"
    name="ReserveBasisExt"
    nullok="true"
    type="varchar">
    <columnParam
      name="size"
      value="600"/>
  </column>
  <column
    desc="Other information pertaining to an evaluation"
    name="OtherInfoExt"
    nullok="true"
    type="varchar">
    <columnParam
      name="size"
      value="600"/>
  </column>
