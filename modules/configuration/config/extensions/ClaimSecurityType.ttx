<?xml version="1.0"?>
<typelistextension
  xmlns="http://guidewire.com/typelists"
  desc="Permissions to restrict access to claims"
  name="ClaimSecurityType">
  <typecode
    code="unsecuredclaim"
    desc="Permission to view claims with no additional security specified"
    name="Unsecure"
    priority="1"/>
  <typecode
    code="employeeclaim"
    desc="Permission to see a claim involving an employee"
    name="Employee claim"
    priority="2"/>
  <typecode
    code="fraudriskclaim"
    desc="Permission to see a claim with a high fraud risk"
    name="Fraud risk"
    priority="2"/>
  <typecode
    code="sensitiveclaim"
    desc="Permission to see a sensitive claim"
    name="Sensitive"
    priority="2"/>
  <typecode
    code="underlitclaim"
    desc="Permission to see a claim that is currently under litigation"
    name="Under litigation"
    priority="2"/>
  <typefilter
    desc="KandR"
    includeAll="true"
    name="KandR">
    <category
      code="FIDCRIME"
      typelist="LossType"/>
  </typefilter>
  <typecode
    code="securedclaimfidcrime"
    desc="Secure Claim"
    name="Secure Claim"
    retired="true"/>
  <typecode
    code="coveragesecure"
    desc="Coverage Secure"
    name="Coverage Secure"
    priority="3"/>
  <typecode
    code="tpasecure"
    desc="External Secure"
    name="External Secure"
    priority="5"/>
  <typecode
    code="secureclaim"
    desc="Secure"
    name="Secure"
    priority="2"/>
  <typecode
    code="secureexternal"
    desc="Assigned Secure"
    name="Assigned Secure"
    priority="4"/>
</typelistextension>