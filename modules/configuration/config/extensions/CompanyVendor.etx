<?xml version="1.0"?>
<!-- Added to include necessary fields for Vendor/Preffered Vendor in address book screens  Defect 424 djohnson - try to change to blank instead of false -->
<extension
  xmlns="http://guidewire.com/datamodel"
  entityName="CompanyVendor">
  <column
    desc="Date of appointment"
    name="ex_AppointmentDate"
    type="datetime"/>
  <column
    default="false"
    desc="Is vendor payable?"
    name="PayableExt"
    nullok="true"
    type="bit"/>
</extension>
