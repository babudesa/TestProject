<?xml version="1.0"?>
<typelistextension
  xmlns="http://guidewire.com/typelists"
  desc="The search field for financial searches"
  name="FinancialSearchField">
  <typecode
    code="Amount"
    desc="Amount"
    name="Amount">
    <category
      code="recovery"
      typelist="SearchObjectType"/>
  </typecode>
  <typecode
    code="futurePaid"
    desc="Future paid"
    name="Future Paid">
    <category
      code="claim"
      typelist="SearchObjectType"/>
  </typecode>
  <typecode
    code="grossCheckAmount"
    desc="Gross amount"
    name="Gross Amount">
    <category
      code="payment"
      typelist="SearchObjectType"/>
  </typecode>
  <typecode
    code="remainingReserve"
    desc="Remaining reserves"
    name="Remaining Reserves">
    <category
      code="claim"
      typelist="SearchObjectType"/>
  </typecode>
  <typecode
    code="totalIncurredNet"
    desc="Net total incurred"
    name="Net Total Incurred">
    <category
      code="claim"
      typelist="SearchObjectType"/>
  </typecode>
  <typecode
    code="totalPaid"
    desc="Total paid"
    name="Total Paid">
    <category
      code="claim"
      typelist="SearchObjectType"/>
  </typecode>
  <typecode
    code="openReserves"
    desc="Open Reserves"
    name="Open Reserves"/>
  <typecode
    code="futurePayments"
    desc="Future Payments"
    name="Future Payments"/>
  <typefilter
    desc="Types to use for the Exposure search"
    name="ExposureSearchTypes">
    <include
      code="futurePayments"/>
    <include
      code="openReserves"/>
    <include
      code="remainingReserve"/>
    <include
      code="totalPaid"/>
  </typefilter>
</typelistextension>