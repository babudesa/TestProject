<?xml version="1.0"?>
<typelistextension
  xmlns="http://guidewire.com/typelists"
  desc="Roles users can have on an assignable object"
  name="UserRole">
  <typecode
    code="attorney"
    desc="Attorney associated with the claim"
    name="Attorney"
    retired="true"/>
  <typecode
    code="doctor"
    desc="Doctor associated with the claim"
    name="Doctor"
    retired="true"/>
  <typecode
    code="indepappr"
    desc="Independent Appraiser associated with the claim"
    name="Independent Appraiser"
    retired="true"/>
  <typecode
    code="investigator"
    desc="Special investigator for the claim"
    name="Corporate Claims Investigator"
    retired="true">
    <category
      code="claimexclusive"
      typelist="UserRoleConstraint"/>
  </typecode>
  <typecode
    code="nursecasemgr"
    desc="Nurse Case Manager associated with the claim"
    name="Nurse Case Manager"
    retired="true"/>
  <typecode
    code="police"
    desc="Police"
    name="Police"
    retired="true"/>
  <typecode
    code="primaryadjuster"
    desc="Primary adjuster for the claim"
    name="Primary Adjuster">
    <category
      code="claimexclusive"
      typelist="UserRoleConstraint"/>
    <category
      code="objectowner"
      typelist="UserRoleConstraint"/>
  </typecode>
  <typecode
    code="propinsp"
    desc="Property Inspector associated with the claim"
    name="Property Inspector"
    retired="true"/>
  <typecode
    code="reinsmgr"
    desc="Reinsurance Manager associated with the claim"
    name="Reinsurance Manager"
    retired="true"/>
  <typecode
    code="relateduser"
    desc="Other user associated with the claim"
    name="Related User"/>
  <typecode
    code="repairshop"
    desc="Repair Shop associated with the claim"
    name="Repair Shop"
    retired="true"/>
  <typecode
    code="reporter"
    desc="Reporter associated with the claim"
    name="Reporter"
    retired="true"/>
  <typecode
    code="salvgowner"
    desc="Salvage Owner associated with the claim"
    name="Salvage Owner"
    retired="true"/>
  <typecode
    code="scoassist"
    desc="Corporate Claims person assisting on the Claim (often high value claims)"
    name="Corporate Claims Assist">
    <category
      code="claimexclusive"
      typelist="UserRoleConstraint"/>
    <category
      code="expexclusive"
      typelist="UserRoleConstraint"/>
  </typecode>
  <typecode
    code="siu"
    desc="SIU Assist associated with the claim"
    name="SIU Assist">
    <category
      code="claimexclusive"
      typelist="UserRoleConstraint"/>
    <category
      code="expexclusive"
      typelist="UserRoleConstraint"/>
  </typecode>
  <typecode
    code="siuinvestigator"
    desc="SIU Investigator associated with the claim"
    name="SIU Investigator"
    retired="true">
    <category
      code="claimexclusive"
      typelist="UserRoleConstraint"/>
  </typecode>
  <typecode
    code="subrogationowner"
    desc="User in charge of subrogation"
    name="Subrogation Owner"
    retired="true">
    <category
      code="expexclusive"
      typelist="UserRoleConstraint"/>
  </typecode>
  <typecode
    code="supervisor"
    desc="Supervisor associated with the claim"
    name="Supervisor"/>
  <typecode
    code="vendor"
    desc="Vendor associated with the claim"
    name="Vendor"
    retired="true"/>
  <typecode
    code="vhcleinsp"
    desc="Vehicle Inspector associated with the claim"
    name="Vehicle Inspector"
    retired="true"/>
  <typecode
    code="profitcenter"
    desc="Profit Center Secure"
    name="Profit Center Secure"
    retired="true"/>
  <typecode
    code="secureclaim"
    desc="Secure Claim"
    name="Secure Claim"/>
  <typecode
    code="sensitiveclaim"
    desc="Sensitive Claim"
    name="Sensitive Claim"/>
  <typecode
    code="tempclaimeditor"
    desc="Temporary Claim Editor"
    name="Temporary Claim Editor"/>
  <typecode
    code="tempclaimviewer"
    desc="Temporary Claim Viewer"
    name="Temporary Claim Viewer"
    retired="true"/>
  <typecode
    code="transferclaim"
    desc="Assigned Secure"
    name="Assigned Secure"/>
  <typecode
    code="secureadjuster"
    desc="Secure Adjuster"
    name="Secure Adjuster"/>
  <typecode
    code="RecoveryAssist"
    desc="Recovery Assist"
    name="Recovery Assist">
    <category
      code="claimexclusive"
      typelist="UserRoleConstraint"/>
    <category
      code="expexclusive"
      typelist="UserRoleConstraint"/>
  </typecode>
  <typefilter
    desc="User Role secure types"
    name="User_Role_secure">
    <include
      code="sensitiveclaim"/>
    <include
      code="tempclaimeditor"/>
    <include
      code="transferclaim"/>
    <include
      code="secureadjuster"/>
    <include
      code="corporateaccount"/>
  </typefilter>
  <typecode
    code="corporateaccount"
    desc="Corporate Accounts"
    name="Corporate Accounts"/>
  <typefilter
    desc="User Role unsecure types"
    name="User_Role_Unsecure">
    <include
      code="sensitiveclaim"/>
    <include
      code="tempclaimeditor"/>
    <include
      code="transferclaim"/>
    <include
      code="secureadjuster"/>
    <include
      code="secureclaim"/>
    <include
      code="corporateaccount"/>
  </typefilter>
  <typecode
    code="billingaddradmin"
    desc="Billing Address Administrator"
    name="Billing Address Administrator"/>
</typelistextension>